<?xml version="1.0" encoding="ISO-8859-1"?>
<portal-config>
  <portal-name>acme</portal-name>
  <locale>en</locale>
  <access-permissions>Everyone</access-permissions>
  <edit-permission>*:/platform/web-contributors</edit-permission>
  <creator>root</creator>
    
 	<portal-layout> 		
 	  <application>
	    <instance-id>portal#acme:/presentation/SiteAdministrationPortlet/acme-admintoolbar</instance-id>
	    <show-info-bar>false</show-info-bar>
  	</application>	  
  	
  	<container id="ACMEBanner" template="system:/groovy/wcm/webui/container/UITableColumnContainer.gtmpl">
  		<container id="ACMELogo" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
  			<application>
  				<portlet>
		      	<application-ref>presentation</application-ref>
		        <portlet-ref>SingleContentViewer</portlet-ref>
		        <preference>
					<name>repository</name>
					<value>repository</value>
					<read-only>false</read-only>
				</preference>
				<preference>
					<name>workspace</name>
					<value>collaboration</value>
					<read-only>false</read-only>
				</preference>
				<preference>
					<name>nodeIdentifier</name>
					<value>/sites content/live/acme/web contents/site artifacts/Logo</value>
					<read-only>false</read-only>
				</preference>
				<preference>
					<name>ShowQuickEdit</name>
					<value>true</value>
					<read-only>false</read-only>
				</preference>
				<preference>
					<name>ShowPrintAction</name>
					<value>false</value>
					<read-only>false</read-only>
				</preference>
		      </portlet>
		      <title>Banner</title>
		      <show-info-bar>false</show-info-bar>
		      <show-application-state>false</show-application-state>
			  <show-application-mode>false</show-application-mode>
			  </application>
  		</container>
  		<container id="ACMESearch" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
