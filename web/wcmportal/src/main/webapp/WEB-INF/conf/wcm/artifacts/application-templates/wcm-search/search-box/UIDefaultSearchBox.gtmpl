<% 
	import org.exoplatform.wcm.webui.search.UISearchBox;
%>
<div class="UIDefaultSearchBox">
<% uiform.begin() %>
	<% String link = uiform.url(uiform.getActions()[0]); %>
	<div class="SearchBox">
		<div class="KeywordInput"><% uiform.renderField(uiform.getChildById(UISearchBox.KEYWORD_INPUT)) %></div>
		<div class="SearchBoxIcon" onclick="$link"><span></span></div>
	</div>
<% uiform.end() %>
</div>