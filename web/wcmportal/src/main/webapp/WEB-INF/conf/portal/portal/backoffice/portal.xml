<?xml version="1.0" encoding="ISO-8859-1"?>
<portal-config>
  <portal-name>backoffice</portal-name>
  <locale>en</locale>
  <access-permissions>Everyone</access-permissions>
  <edit-permission>*:/platform/web-contributors</edit-permission>
  <creator>root</creator>
    
 	<portal-layout> 		
 	  <application>
	    <instance-id>portal#backoffice:/web-presentation/SiteAdministrationPortlet/backoffice-admintoolbar</instance-id>
	    <show-info-bar>false</show-info-bar>
  	</application>	  
  	
  	<container id="BannerContainer" template="system:/groovy/wcm/webui/container/UITableColumnContainer.gtmpl">
  		<container id="Breadcrumb" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
		  	<application>
					<instance-id>portal#backoffice:/web-presentation/SingleContentViewer/backoffice-breadcrumb</instance-id>
			  	<show-info-bar>false</show-info-bar>
			  </application>
  		</container>
  		<container id="WCMSearchBox" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
	  		<application>
			    <instance-id>portal#backoffice:/web-presentation/SingleContentViewer/backoffice-searchbox</instance-id>
			    <show-info-bar>false</show-info-bar>
			  </application>
  		</container>
  	</container>
		
	  <page-body> </page-body>
	  
	  <application>
	    <instance-id>portal#backoffice:/web-presentation/SingleContentViewer/backoffice-footer</instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>

  </portal-layout>
  
</portal-config>
