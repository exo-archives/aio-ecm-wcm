<?xml version="1.0" encoding="ISO-8859-1"?>
<portal-config>
  <portal-name>classic</portal-name>
  <locale>en</locale>
  <access-permissions>Everyone</access-permissions>
  <edit-permission>*:/platform/administrators</edit-permission>
  <creator>root</creator>
    
  <portal-layout> 		
 	  <application>
	    <instance-id>portal#classic:/web-presentation/SiteAdministrationPortlet/classic-admintoolbar</instance-id>
	    <show-info-bar>false</show-info-bar>
      </application>	  	
      
	  <application>
	    <instance-id>portal#classic:/iweb/BannerPortlet/classic-banner</instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
	  
	  <application>
	  	<instance-id>portal#classic:/web/NavigationPortlet/classic-toolbar</instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
	  
	  <application>
		<instance-id>portal#classic:/web/BreadcumbsPortlet/classic-breadcumbs</instance-id>
	  	<show-info-bar>false</show-info-bar>
	  </application>	 
	  	  
	  <page-body> </page-body>
	  
	  <application>
	    <instance-id>portal#classic:/iweb/FooterPortlet/classic-footer</instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
  </portal-layout>
  
</portal-config>
