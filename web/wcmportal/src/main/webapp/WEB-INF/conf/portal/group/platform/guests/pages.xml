<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>
	
	<page>
  	<page-id>group::platform/guests::webos</page-id>
  	<owner-type>group</owner-type>
		<owner-id>platform/guests</owner-id>
    <name>webos</name>		
    <factory-id>Desktop</factory-id>
	  <access-permissions>*:/platform/guests</access-permissions>
	  <edit-permission>*:/platform/administrators</edit-permission>
    <show-max-window>true</show-max-window>
    
    <application>
	    <instance-id>group#platform/guests:/eXoWidgetWeb/WelcomeWidget/WelcomeWidget</instance-id>
	    <application-type>eXoWidget</application-type>
	    <title>Welcome Widget</title>
	    <show-info-bar>true</show-info-bar>
      <show-application-state>true</show-application-state>
      <show-application-mode>true</show-application-mode>
	  </application>
	  
	  <application>
      <instance-id>group#platform/guests:/exoadmin/AccountPortlet/webosAccount</instance-id>    
      <title>Register</title>
      <show-info-bar>true</show-info-bar>
      <show-application-state>true</show-application-state>
      <show-application-mode>true</show-application-mode>
    </application>
            
    <application>
      <instance-id>group#platform/guests:/web/BrowserPortlet/webosWebExplorer</instance-id>    
      <title>Web Explorer</title>
      <show-info-bar>true</show-info-bar>
      <show-application-state>true</show-application-state>
      <show-application-mode>true</show-application-mode>
    </application>                   
        
    <application>
      <instance-id>group#platform/guests:/web/SiteMapPortlet/SitemapPortlet/webosSiteMap</instance-id>    
      <title>Site Map</title>
    </application>        
  </page>
  		
  <page>
  	<page-id>group::platform/guests::register</page-id>
    <owner-type>group</owner-type>
    <owner-id>platform/guests</owner-id>
    <name>register</name>
    <title>Register</title>
	  <access-permissions>*:/platform/guests</access-permissions>
	  <edit-permission>*:/platform/administrators</edit-permission>    
    <application>
      <instance-id>group#platform/guests:/exoadmin/AccountPortlet/Account</instance-id>
      <title>Account</title>
      <show-info-bar>false</show-info-bar>      
    </application>  
  </page>    
        
  <page>
  	<page-id>group::platform/guests::sitemap</page-id>
    <owner-type>group</owner-type>
    <owner-id>platform/guests</owner-id>
    <name>sitemap</name>
    <title>Site Map</title>
	  <access-permissions>*:/platform/guests</access-permissions>	  
	  <edit-permission>*:/platform/administrators</edit-permission>
	  <application>
      <instance-id>group#platform/guests:/web/SiteMapPortlet/sitemapportlet</instance-id>
      <title>Site Map</title>
      <show-info-bar>false</show-info-bar>      
    </application>          
  </page>
    
</page-set>

