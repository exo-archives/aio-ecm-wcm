<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>  
  <page>
  	<page-id>portal::backoffice::dashboard</page-id>
    <owner-type>portal</owner-type>
    <owner-id>backoffice</owner-id>
    <name>dashboard</name>
    <title>Dashboard</title>
	  <access-permissions>Everyone</access-permissions>
	  <edit-permission>manager:/platform/web-contributors</edit-permission>
	  <container id="emptyHomepage" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
	  	<application>
	      <instance-id>portal#backoffice:/web-presentation/SingleContentViewer/backoffice-homepage</instance-id>
	      <title>Dashboard</title>
	    </application>
	  </container>
  </page>
  
  <page>
  	<page-id>portal::backoffice::parameterizedviewer</page-id>
    <owner-type>portal</owner-type>
    <owner-id>backoffice</owner-id>
    <name>parameterizedviewer</name>
    <title>ParameterizedViewer</title>
    <access-permissions>Everyone</access-permissions> 
    <edit-permission>manager:/platform/web-contributors</edit-permission>
    <application>
      <instance-id>portal#backoffice:/web-presentation/ParameterizedContentViewerPortlet/backoffice-parameterizedviewer</instance-id>
      <title>parameterizedviewer</title>
      <show-info-bar>false</show-info-bar>      
      <show-application-state>false</show-application-state>
      <show-application-mode>false</show-application-mode>
    </application>
  </page>
  <page>
  	<page-id>portal::backoffice::printviewer</page-id>
    <owner-type>portal</owner-type>
    <owner-id>backoffice</owner-id>
    <name>printviewer</name>
    <title>printviewer</title>
    <access-permissions>Everyone</access-permissions> 
    <edit-permission>manager:/platform/web-contributors</edit-permission>
    <show-max-window>true</show-max-window>
    <application>
      <instance-id>portal#backoffice:/web-presentation/ParameterizedContentViewerPortlet/backoffice-printviewer</instance-id>
      <title>printviewer</title>
      <show-info-bar>false</show-info-bar>      
      <show-application-state>false</show-application-state>
      <show-application-mode>false</show-application-mode>
    </application>
  </page>
</page-set>
