<?xml version="1.0" encoding="UTF-8"?>
<bundle>
 <UIExoStart>
  <tooltip>
   <homepage>Homepage</homepage>
  </tooltip>
 </UIExoStart>
 
 <UIFormInputSetWithAction>
  <msg>
   <confirm-delete>Are you sure want to delete ?</confirm-delete>
  </msg>
 </UIFormInputSetWithAction>
 
 <Drives>
  <label>
   <ManagedSites>Sites Management</ManagedSites>
   <SharedSite>Shared Site</SharedSite>
  </label>
 </Drives>
 
 <Views>
  <label>
   <wcm-view>WCM View</wcm-view>
   <admin-view>Admin View</admin-view>
   <icon-view>Icon View</icon-view>
   <thumbails-view>Thumbnails View</thumbails-view>
   <cover-flow>Cover Flow View</cover-flow>
  </label>
 </Views>
 
 <UIPageSelectorPanel>
  <header>
   <path>Path</path>
   <action>Action</action>
  </header>
  <msg>
   <child-not-found>No page found</child-not-found>
  </msg>
 </UIPageSelectorPanel>
 
 <UIContentDialogFormPopupWindow>
  <title>
   <UIContentDialogPreference>Webcontent preference</UIContentDialogPreference>
  </title>
 </UIContentDialogFormPopupWindow>
 
 <UITabPane>
  <title>
   <UIContentBrowsePanelOne>Content browser</UIContentBrowsePanelOne>
   <UIContentBrowsePanelMulti>Content browser</UIContentBrowsePanelMulti>
   <UIContentBrowsePanelFolder>Folder browser</UIContentBrowsePanelFolder>
   <UIContentSearchForm>Content search form</UIContentSearchForm>
   <UIContentSearchResult>Search result</UIContentSearchResult>
   <UIContentViewer>Preview content</UIContentViewer>
   <WebContentMetadataPopup>Property selector</WebContentMetadataPopup>
   <WebContentNodeTypePopup>Nodetype selector</WebContentNodeTypePopup>
   <UIPermissionManager>Permission manager</UIPermissionManager>
  </title>
 </UITabPane>
 
 <UIContentBrowsePanelOne>
  <selectBoxValues>
   <FilterBy>Filter by</FilterBy>
   <WebContent>Web Content</WebContent>
   <DMSDocument>Document</DMSDocument>
   <Media>Media</Media>
  </selectBoxValues>
 </UIContentBrowsePanelOne>
 
 <UIContentBrowsePanelMulti>
  <selectBoxValues>
   <FilterBy>Filter by</FilterBy>
   <WebContent>Web Content</WebContent>
   <DMSDocument>Document</DMSDocument>
   <Media>Media</Media>
  </selectBoxValues>
 </UIContentBrowsePanelMulti>
  
 <UIContentBrowsePanelOne>
  <label>
   <LeftColumn>Select a folder to display its children as content</LeftColumn>
   <RightColumn>Below is the possible content can be selected</RightColumn>
  </label>
 </UIContentBrowsePanelOne>
 
 <UIContentBrowsePanelMulti>
  <label>
   <LeftColumn>Select a folder to display its children as content</LeftColumn>
   <RightColumn>Below is the possible content can be selected</RightColumn>
  </label>
 </UIContentBrowsePanelMulti>
 
 <UIContentBrowsePanelFolder>
  <label>
   <LeftColumn>Select a folder to display its children as content</LeftColumn>
   <RightColumn>Below is the possible folder can be selected</RightColumn>
  </label>
 </UIContentBrowsePanelFolder>
 
 <UISelectedContentGrid>
  <label>
   <name>Name</name>
   <add>Add</add>
   <delete>Delete</delete>  		  		
  </label>
  <msg>
   <no-content-found>No content found</no-content-found>
   <non-content>You didn't select any content</non-content>
  </msg>
 </UISelectedContentGrid>
 
 <UICLVContentSelectedGrid>
  <action>
   <SaveCategories>Save</SaveCategories>
   <Cancel>Cancel</Cancel>
  </action>
 </UICLVContentSelectedGrid>
 
 <UIContentBrowsePathSelector>
  <header>
   <name>Name</name>
   <publicationstate>Publication state</publicationstate>
   <action>Action</action>
  </header>
  <msg>
   <child-not-found>No content found</child-not-found>
  </msg>
 </UIContentBrowsePathSelector>
 
 <UIContentBrowsePathSelectorFolder>
  <header>
   <name>Name</name>
   <publicationstate>Publication state</publicationstate>
   <action>Action</action>
  </header>
  <msg>
   <child-not-found>No folder found</child-not-found>
  </msg>
 </UIContentBrowsePathSelectorFolder>
 
 <UIContentSearchForm>
   <title>
    <Location>Site Name</Location>
    <SearchByName>Search by name</SearchByName>
    <Fulltext>A word or phrase in context</Fulltext>
    <Property>Property</Property>
    <Contain>Contain</Contain>
    <FromDate>from date</FromDate>
    <ToDate>to date</ToDate>
    <DocumentType>Document Type</DocumentType>
   </title>
   <label>
    <option>
     <CREATED>Created</CREATED>
     <MODIFIED>Modified</MODIFIED>
    </option>
   </label>
   <action>
    <SearchWebContent>Search</SearchWebContent>
   </action>
   <msg>
    <empty-field>Please input valid keyword for search</empty-field>
    <invalid-format>Invalid format datetime in field '{0}'</invalid-format>
    <invalid-date>"To Date" have to greater than "From Date"</invalid-date>
    <invalid-nodeType>Node type "{0}" is invalid! Please check again.</invalid-nodeType>
   </msg>
  </UIContentSearchForm>
  
  <WebContentMetadataPopup>
   <title>
    <UIContentPropertySelector>Property Selector</UIContentPropertySelector>
   </title>
  </WebContentMetadataPopup>
  
  <UIContentPropertySelector>
   <action>
    <Add>Add</Add>
    <Cancel>Cancel</Cancel>
   </action>
   <label>
    <metadataType>Metadata type</metadataType>
    <property_select>Property</property_select>
   </label>
  </UIContentPropertySelector>
  
  <WebContentNodeTypePopup>
   <title>
    <UIContentNodeTypeSelector>Nodetype Selector</UIContentNodeTypeSelector>
   </title>
  </WebContentNodeTypePopup>
  
  <UIContentNodeTypeSelector>
   <action>
    <Save>Save</Save>
    <Cancel>Cancel</Cancel>
   </action>
  </UIContentNodeTypeSelector>
  
  <UIContentSearchResult>
   <header>
    <title>Title</title>
    <score>Score</score>
    <publicationstate>Publication state</publicationstate>
    <action>Action</action>
   </header>
   <title>
    <Select>Select</Select>
    <View>View</View>
   </title>
  </UIContentSearchResult>
  
  <UIContentDialogForm>
   <action>
    <SaveDraft>Save Draft</SaveDraft>
    <FastPublish>Fast Publish</FastPublish>
    <Preferences>Preferences</Preferences>
    <Close>Close</Close>
   </action>
  </UIContentDialogForm>
  
  <UIPermissionManager>
   <label>
    <UIPermissionInputSetWithAction>Permission</UIPermissionInputSetWithAction>
    <UIPermissionStringInput></UIPermissionStringInput>
    <UIAccessibleCheckboxInput>Access Right</UIAccessibleCheckboxInput>
    <UIEditableCheckboxInput>Edit Right</UIEditableCheckboxInput>
   </label>
   <action>
    <Save>Save</Save>
    <Clear>Clear</Clear>
    <title>
     <Edit>Edit</Edit>
     <Delete>Delete</Delete>
    </title>
   </action>
  </UIPermissionManager>
  
  <UIPermissionManagerGrid>
   <header>
    <owner>Permission</owner>
    <accessible>Accessible</accessible>
    <editable>Editable</editable>
    <action>Action</action>
   </header>
   <msg>
    <confirm-delete-permission>Are you sure to delete ?</confirm-delete-permission>
    <no-permission-remove>Don't have permission.</no-permission-remove>
    <access-denied>Access denied.</access-denied>
    <userOrGroup-required>Require select user or group.</userOrGroup-required>
    <checkbox-require>You should set right.</checkbox-require>
    <node-locked>The item with path {0} or upper level is locked.</node-locked>
    <node-checkedin>You must check out first</node-checkedin>
   </msg>
  </UIPermissionManagerGrid>
  
  <UIPermissionInputSetWithAction>
   <title>
    <SelectUser>Select user</SelectUser>
    <SelectMember>Select member</SelectMember>
    <AddAny>Add any permission</AddAny>
   </title>
  </UIPermissionInputSetWithAction>
  
  <UIUserSelectorPopupWindow>
   <title>
    <UIUserContainer>User selector</UIUserContainer>
   </title>
  </UIUserSelectorPopupWindow>
  
  <UIGroupSelectorPopupWindow>
   <title>
    <UIGroupMemberSelector>Group selector</UIGroupMemberSelector>
   </title>
  </UIGroupSelectorPopupWindow>
  
  <UIGroupMemberSelector>
   <label>
    <selectGroup>Select group</selectGroup>
    <selectMembership>Select membership</selectMembership>
   </label>
  </UIGroupMemberSelector>
  
  <UIContentDialogPreference>
   <action>
    <Back>Back</Back>
   </action>
  </UIContentDialogPreference>
  
  <ZeroValidator>
   <msg>
    <Invalid-number>Zero is not accepted in field '{0}'</Invalid-number>
   </msg>
  </ZeroValidator>
</bundle>