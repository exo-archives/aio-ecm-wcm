    </label>
    <action>
			<Save>Save</Save>
	    <Clear>Clear</Clear>
		</action>
	</UIPermissionManager>
	  
  <UIPermissionManagerGrid>
    <action>
    	<Save>Save</Save>
      <Reset>Reset</Reset>
      <title>
        <Edit>Edit</Edit>
        <Delete>Delete</Delete>
      </title>
    </action>
    <header>
      <owner>Permission</owner>
      <accessible>Accessible</accessible>
