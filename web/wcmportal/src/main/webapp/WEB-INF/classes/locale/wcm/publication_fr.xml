<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIPublishingPanel>
    <panelLeft>
      <title>Ci-dessous sont les pages possibles où le contenu peut être affiché</title>
    </panelLeft>
    <panelRight>
      <title>Ci-dessous sont les pages où le contenu est affiché</title>
    </panelRight>
    <currentPortal>
      <title>Site courant</title>
      <none>Aucun</none>
    </currentPortal>
    <currentNode>
      <title>Noeud courant</title>
      <none>Aucun</none>
    </currentNode>
  </UIPublishingPanel>
  <UIPublicationComponentStatus>
    <action>
      <Close>Fermer</Close>
    </action>
  </UIPublicationComponentStatus>
  <UIPopupWindow>
