<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIPublishingPanel>
    <panelLeft>
      <title>Nội dung này có thể được xuất bản vào những trang dưới đây</title>
    </panelLeft>
    <panelRight>
      <title>Nội dung này đã được xuất bản vào những trang dưới đây</title>
    </panelRight>
    <currentPortal>
      <title>Site hiện tại</title>
      <none>Không có gì</none>
    </currentPortal>
    <currentNode>
      <title>Node hiện tại</title>
      <none>Không có gì</none>
    </currentNode>
  </UIPublishingPanel>
  <UIPublicationComponentStatus>
    <action>
      <Close>Đóng</Close>
    </action>
  </UIPublicationComponentStatus>
  <UIPopupWindow>
  	<title>
  		<UIPublicationContainer>Quản lý xuất bản</UIPublicationContainer>
  	</title>
  </UIPopupWindow>
  <UITabPane>
    <title>
      <UIPublishingPanel>Form xuất bản</UIPublishingPanel>
      <UIPublicationPanel>Trạng thái xuất bản</UIPublicationPanel>
      <UIPublicationPagesContainer>Xuất bản vào trang</UIPublicationPagesContainer>
      <UIPublicationHistory>Nhật ký xuất bản</UIPublicationHistory>
      <UIVersionViewer>Xem lại phiên bản</UIVersionViewer>
    </title>
  </UITabPane>
  <UIClvPopupContainer>
  	<title>
  		<UIClvPopupContainer>Content list viewer popup container</UIClvPopupContainer>
  		<UIPublishClvChooser>Chọn CLV portlet</UIPublishClvChooser>
  	</title>
  	<action>
 			<close>Đóng</close>
 		</action>
  </UIClvPopupContainer>
  <UIPublicationAction>
    <msg>
      <none>Trang nội dung này đã được lựa chọn</none>
      <duplicate>Nội dung đã được xuất bản vào trang này</duplicate>
      <wrongNode>Đây không phải là một trang nội dung, hãy chọn cái khác</wrongNode>
    </msg>
    <action>
      <add>Xuất bản nội dung vào trang</add>
      <remove>Xóa nội dung ra khỏi trang</remove>
    </action>
  </UIPublicationAction>
  <UIPublishedPages>
    <label>
      <noitem>Nội dung này chưa được xuất bản vào trang nào</noitem>
    </label>
  </UIPublishedPages>
  <UIPublicationPanel>
  	<button>
  		<close>Đóng</close>
  	</button>
  	<revisions>
  		<version>Phiên bản</version>
  		<date>Ngày tháng</date>
  		<author>Tác giả</author>
  		<status>Trạng thái</status>
  		<action>Hành động</action>
  		<label>Phiên bản</label>
  		<none>Không có phiên bản nào</none>
  		<seeall>Xem tất cả các phiên bản</seeall>
  		<currentVersion>phiên bản hiện tại</currentVersion>
  		<confirm>Bạn sẽ mất những gì đã thay đổi trong phiên bản nháp, bạn có thực sự muốn khôi phục lại phiên bản này và thay thế cho phiên bản hiện tại ?</confirm>
  	</revisions>
  	<status>
  		<version>Phiên bản</version>
  		<title>Trạng thái</title>
  		<enrolled>Đã được biên soạn</enrolled>
  		<draft>Nháp</draft>
  		<awaiting>Đang chờ</awaiting>
  		<live>Live</live>
  		<obsolete>Hết hạn</obsolete>
  	</status>
  	<scheduled>
  		<title>Lịch</title>
  		<from>từ</from>
  		<to>đến</to>
  	</scheduled>
  	<title>
  		<preview>Xem lại</preview>
  		<restore>Khôi phục</restore>
  	</title>
  	<msg>
    	<have-no-view-template>Mẫu view của node này không được hỗ trợ</have-no-view-template>
    	<restore-complete>Khôi phục thành công</restore-complete>
    </msg>
  </UIPublicationPanel>
  <UIPublicationHistory>
  	<header>
  		<date>Ngày tháng</date>
  		<versionName>Tên phiên bản</versionName>
  		<currentState>Trạng thái hiện tại</currentState>
  		<user>Người tạo</user>
  		<description>Mô tả</description>
  	</header>
  </UIPublicationHistory>
  <Publication>
  	<log>
	  	<description>
	  		<enrolled>Nội dung đã được tham gia</enrolled>
	  		<change-to-draft-state>Nội dung đã được tạo</change-to-draft-state>
	  		<change-to-live-state>từ bản Nháp sang bản Live</change-to-live-state>
	  		<change-to-obsolete-state>từ bản Live sang Hết hạn</change-to-obsolete-state>
	  	</description>
  	</log>
  </Publication>
</bundle>