<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIPublishingPanel>
    <panelLeft>
      <title>Nội dung này có thể được xuất bản vào những trang dưới đây</title>
    </panelLeft>
    <panelRight>
      <title>Nội dung này đã được xuất bản vào những trang dưới đây</title>
    </panelRight>
    <currentPortal>
      <title>Site hiện tại</title>
      <none>Không có gì</none>
    </currentPortal>
    <currentNode>
      <title>Node hiện tại</title>
      <none>Không có gì</none>
    </currentNode>
  </UIPublishingPanel>
  <UIPublicationComponentStatus>
    <action>
      <Close>Đóng</Close>
    </action>
  </UIPublicationComponentStatus>
  <UIPopupWindow>
  	<title>
  		<UIPublicationContainer>Quản lý xuất bản</UIPublicationContainer>
  	</title>
  </UIPopupWindow>
  <UITabPane>
    <title>
      <UIPublishingPanel>Form xuất bản</UIPublishingPanel>
      <UIPublicationPanel>Trạng thái xuất bản</UIPublicationPanel>
      <UIPublicationPages>Xuất bản vào trang</UIPublicationPages>
      <UIPublicationHistory>Nhật ký xuất bản</UIPublicationHistory>
    </title>
  </UITabPane>
  <UIPublicationAction>
    <msg>
      <none>Trang nội dung này đã được lựa chọn</none>
      <duplicate>Nội dung đã được xuất bản vào trang này</duplicate>
      <wrongNode>Đây không phải là một trang nội dung, hãy chọn cái khác</wrongNode>
    </msg>
    <action>
      <add>Xuất bản nội dung vào trang</add>
      <remove>Xóa nội dung ra khỏi trang</remove>
    </action>
  </UIPublicationAction>
  <UIPublishedPages>
    <label>
      <noitem>Nội dung này chưa được xuất bản vào trang nào</noitem>
    </label>
  </UIPublishedPages>
  <UIPublicationPanel>
  	<button>
  		<close>Đóng</close>
  	</button>
  	<revisions>
  		<version>Phiên bản</version>
  		<date>Ngày tháng</date>
  		<author>Tác giả</author>
  		<status>Trạng thái</status>
  		<action>Hành động</action>
  		<label>Phiên bản</label>
  		<none>Không có phiên bản nào</none>
  		<seeall>Xem tất cả các phiên bản</seeall>
  	</revisions>
  	<status>
  		<version>Phiên bản</version>
  		<title>Trạng thái</title>
  		<enrolled>Đã được biên soạn</enrolled>
  		<draft>Nháp</draft>
  		<awaiting>Đang chờ</awaiting>
  		<live>Sống</live>
  		<obsolete>Hết hạn</obsolete>
  	</status>
  	<scheduled>
  		<title>Lịch</title>
  		<from>từ</from>
  		<to>đến</to>
  	</scheduled>
  </UIPublicationPanel>
</bundle>