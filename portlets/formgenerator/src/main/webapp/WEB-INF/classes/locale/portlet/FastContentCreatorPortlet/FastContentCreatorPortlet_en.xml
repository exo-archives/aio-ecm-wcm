<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<!-- View mode -->
	<UIFastContentCreatorForm>
    <msg>
      <name-not-allowed>This name is not allowed.</name-not-allowed>
      <in-versioning>Need to checkout current node before add new node</in-versioning>
      <cannot-save>Some constraints were violated during that action.</cannot-save>
      <saved-successfully>This node has been saved at '{0}' location successfully</saved-successfully>
      <not-support>Cannot load template of '{0}'.</not-support>
      <not-add-folder>Only nt:file can add in this folder</not-add-folder>
      <access-denied>Access denied! You haven't got add permission at '{0}'</access-denied>
      <path-not-found>Path '{0}' was deleted or is invalid path.</path-not-found>
      <node-locked>Node '{0}' is locked. Unlock this node before create documents.</node-locked>
      <node-isExist>This node already exists in '{0}' and same name sibling is not allowed</node-isExist>
    </msg>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
  </UIFastContentCreatorForm>
	
	<!-- Edit mode -->
  <UIFastContentCreatorConfig>
    <label>
			<select-repository>Select repository</select-repository>
			<UIFastContentCreatorSaveLocationField>Save location</UIFastContentCreatorSaveLocationField>
      <UIFastContentCreatorRepositoryFormSelectBox>Select repository</UIFastContentCreatorRepositoryFormSelectBox>
      <UIFastContentCreatorWorkspaceFormSelectBox>Select workspace</UIFastContentCreatorWorkspaceFormSelectBox>
			<UIFastContentCreatorTemplateField>Template options</UIFastContentCreatorTemplateField>
      <UIFastContentCreatorLocationFormInputAction>Location to save</UIFastContentCreatorLocationFormInputAction>
			<UIFastContentCreatorLocationFormStringInput></UIFastContentCreatorLocationFormStringInput>
      <UIFastContentCreatorTemplateFormSelectBox>Select template</UIFastContentCreatorTemplateFormSelectBox>
			<UIFastContentCreatorSaveFormStringInput>Custom save button</UIFastContentCreatorSaveFormStringInput>
			<UIFastContentCreatorMessageFormTextareaInput>Custom save message</UIFastContentCreatorMessageFormTextareaInput>
      <UIFastContentCreatorRedirectFormCheckboxInput>Redirect</UIFastContentCreatorRedirectFormCheckboxInput>
			<UIFastContentCreatorRedirectPathFormStringInput>Redirect path</UIFastContentCreatorRedirectPathFormStringInput>
			<UIFastContentCreatorActionField>Actions</UIFastContentCreatorActionField>
			<UIFastContentCreatorActionList></UIFastContentCreatorActionList>
    </label>
    <msg>
      <save-successfully>Saved to portlet preferences successfully</save-successfully>
      <item-not-found>This path is invalid!</item-not-found>
      <fileType-empty>You have to choose file type before save this configuration.</fileType-empty>
      <ws-empty>You have to choose one workspace.</ws-empty>
    </msg>
    <action>
      <Save>Save</Save>
    </action>
  </UIFastContentCreatorConfig>
  
	<UIFastContentCreatorLocationFormInputAction>
		<title>
			<SelectPath>Select path</SelectPath>
		</title>
	</UIFastContentCreatorLocationFormInputAction>

	<UIFastContentCreatorActionList>
		<header>
			<name>Name</name>
			<description>Description</description>
			<instanceOf>Instance of</instanceOf>
			<action>Action</action>
		</header>
		<action>
			<Add>Add</Add>
		</action>
		<msg>
			<empty>No action</empty>
      <remove-popup-first>You must close edit popup before delete this node.</remove-popup-first>
      <confirm-delete-action>Do yo really want to delete this action?</confirm-delete-action>
      <template-empty>Template of '{0}' is null or empty</template-empty>
      <template-null>Template is null or you haven't got permission to access this template.</template-null>
      <access-denied>Access denied! You haven't got enough permission to do this action.</access-denied>
    </msg>
	</UIFastContentCreatorActionList>
	
	<!-- Select path popup -->
	<UIFastContentCreatorSelectorPopupWindow>
    <title>
      <UIOneNodePathSelector>Select location</UIOneNodePathSelector>
    </title>
  </UIFastContentCreatorSelectorPopupWindow>	
	
	<UIWorkspaceList>
    <label>
      <workspaceName>Workspace</workspaceName>
      <rootNodePath>Root path</rootNodePath>
			<rootNodeInfo></rootNodeInfo>
    </label>
  </UIWorkspaceList>
	
	<rootNodeInfo>
		<title>
			<AddRootNode>Add root node</AddRootNode>
		</title>
	</rootNodeInfo>
	
  <UITreeList>
    <info>
      <child-not-found>This node hasn't got any child</child-not-found>
    </info>
    <header>
      <name>Name</name>
      <action>Action</action>
    </header>
  </UITreeList>
	
	<!-- Add action popup -->
	<UIFastContentCreatorActionPopupWindow>
		<title>
			<UIFastContentCreatorActionContainer>Add action form</UIFastContentCreatorActionContainer>
		</title>
	</UIFastContentCreatorActionPopupWindow>
	
	<UIFastContentCreatorActionForm>
		<title>Add/Edit an Action</title>
		<action>
			<Save>Save</Save>
			<Close>Close</Close>
		</action>
		<msg>
      <repository-exception>Can not save this action. The name provided must not have an index on its final element.</repository-exception>
      <numberformat-exception>Can not save this action. The name provided must not have '[' or ']'.</numberformat-exception>
      <existed-action>The action name '{0}' is existing so choose another name please!</existed-action>
      <unable-add-action>Unable add this action</unable-add-action>
      <unable-add>Unable add this action! This node is root or you don't have enough permision</unable-add>
      <not-support>The dialog '{0}' is not supported</not-support>
      <no-permission-add>Access denied ! You have no right to add action here</no-permission-add>
      <access-denied>Access denied! You haven't got permission to access this template.</access-denied>
    </msg>
		<validator>
      <validator>Validator</validator>
      <destPath>With path</destPath>
    </validator>
	</UIFastContentCreatorActionForm>
		
	<UIFastContentCreatorActionTypeForm>
		<label>
			<actionType>Create an Action of Type</actionType>
		</label>
		<msg>
  		<not-support>Cannot load template of '{0}'.</not-support>
  		<do-not-permission>You don't have permission to load template of '{0}'.</do-not-permission>
  	</msg>
	</UIFastContentCreatorActionTypeForm>
	
  <EmptyFieldValidator>
    <msg>
      <empty-input>Type value into field '{0}' please</empty-input>
    </msg>
  </EmptyFieldValidator>
	
	


	<!-- check -->
  <PopupComponent>
    <title>
      <UIOneNodePathSelector>Select location</UIOneNodePathSelector>
      <UIPermissionSelector>Permission Browser</UIPermissionSelector>
      <UICategoriesSelector>Add Categories</UICategoriesSelector>
    </title>
  </PopupComponent>
	
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Do you want to delete?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <DateTimeValidator>
    <msg>
      <Invalid-input>Field '{0}' with value '{1}' is invalid</Invalid-input>
    </msg>
  </DateTimeValidator>
  <ECMNameValidator>
    <msg>
      <Invalid-char>There are some invalid characters in field '{0}'. Type another value, please!</Invalid-char>
      <empty-input>The value of field '{0}' is null or empty!</empty-input>
    </msg>
  </ECMNameValidator>
  
  <UITabPane>
    <title>
      <UICategoriesSelectPanel>Select categories</UICategoriesSelectPanel>
      <UISelectedCategoriesGrid>Referenced categories</UISelectedCategoriesGrid>
    </title>
  </UITabPane>
  <PopupComponent>
    <title>
      <UICategoriesSelector>Add Categories</UICategoriesSelector>
    </title>
  </PopupComponent>
  <UICategoriesSelector>
    <action>
      <close>Close</close>
    </action>
  </UICategoriesSelector>

  <PopupComponent>
    <title>
      <UIOneTaxonomySelector>Add Categories</UIOneTaxonomySelector>
    </title>
  </PopupComponent>
  
  <UITreeTaxonomyList>
    <label>
      <taxonomyTree>Taxonomy Tree</taxonomyTree>
    </label>
  </UITreeTaxonomyList>
  
  
</bundle>