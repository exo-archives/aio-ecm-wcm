<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<!-- View mode -->
	<UIFastContentCreatorForm>
    <msg>
      <name-not-allowed>Ce nom n'est pas autorisée.</name-not-allowed>
      <in-versioning>Besoin de courant à la caisse avant d'ajouter un nouveau noeud noeud</in-versioning>
      <cannot-save>Certaines contraintes ont été violés au cours de cette action.</cannot-save>
      <saved-successfully>Ce nœud a été enregistré à '{0}' emplacement avec succès</saved-successfully>
      <not-support>Impossible de charger le modèle de '{0}'.</not-support>
      <not-add-folder>nt:file peut seulement ajouter dans ce dossier</not-add-folder>
      <access-denied>Accès refusé! Vous n'avez pas la permission d'ajouter à '{0}'</access-denied>
      <path-not-found>Path '{0}' a été supprimé ou est invalide chemin.</path-not-found>
      <node-locked>PathNode '{0}' est verrouillé. Unlock ce noeud avant de créer des documents.</node-locked>
      <node-isExist>Ce nœud existe déjà dans '{0}' et le même nom de frère n'est pas autorisé</node-isExist>
    </msg>
    <action>
      <Save>Sauver</Save>
      <Cancel>Annuler</Cancel>
    </action>
  </UIFastContentCreatorForm>
	


	<!-- Edit mode -->
  <UIFastContentCreatorConfig>



    <label>
			<select-repository>Sélectionnez référentiel</select-repository>
			<UIFastContentCreatorSaveLocationField>Enregistrer le lieu</UIFastContentCreatorSaveLocationField>
      <UIFastContentCreatorRepositoryFormSelectBox>Sélectionnez référentiel</UIFastContentCreatorRepositoryFormSelectBox>
      <UIFastContentCreatorWorkspaceFormSelectBox>Sélectionnez travail</UIFastContentCreatorWorkspaceFormSelectBox>
			<UIFastContentCreatorTemplateField>Les options du modèle</UIFastContentCreatorTemplateField>
      <UIFastContentCreatorLocationFormInputAction>Lieu à mettre</UIFastContentCreatorLocationFormInputAction>
			<UIFastContentCreatorLocationFormStringInput></UIFastContentCreatorLocationFormStringInput>
      <UIFastContentCreatorTemplateFormSelectBox>Sélectionner un modèle</UIFastContentCreatorTemplateFormSelectBox>
			<UIFastContentCreatorSaveFormStringInput>Personnaliser le bouton Enregistrer</UIFastContentCreatorSaveFormStringInput>
			<UIFastContentCreatorMessageFormTextareaInput>Custom mettre message</UIFastContentCreatorMessageFormTextareaInput>
      <UIFastContentCreatorRedirectFormCheckboxInput>Redirect</UIFastContentCreatorRedirectFormCheckboxInput>
			<UIFastContentCreatorRedirectPathFormStringInput>Redirect chemin</UIFastContentCreatorRedirectPathFormStringInput>
			<UIFastContentCreatorActionField>Actions</UIFastContentCreatorActionField>
			<UIFastContentCreatorActionList></UIFastContentCreatorActionList>






    </label>
    <msg>
      <save-successfully>Enregistré dans les préférences de portlet avec succès</save-successfully>
      <item-not-found>Ce chemin n'est pas valide!</item-not-found>
      <fileType-empty>Vous devez choisir le type de fichier avant d'enregistrer cette configuration.</fileType-empty>
      <ws-empty>Vous devez choisir l'une de travail.</ws-empty>
    </msg>
    <action>
      <Save>Sauver</Save>
    </action>
  </UIFastContentCreatorConfig>
  
	<UIFastContentCreatorLocationFormInputAction>


		<title>
			<SelectPath>Sélectionnez chemin</SelectPath>

		</title>
	</UIFastContentCreatorLocationFormInputAction>

	<UIFastContentCreatorActionList>
		<header>



			<name>Nom</name>
			<description>Description</description>
			<instanceOf>Instance de</instanceOf>
			<action>Action</action>
		</header>










		<action>
			<Add>Ajouter</Add>


		</action>


		<msg>
			<empty>Aucune action</empty>
      <remove-popup-first>Vous devez fermer popup modifier avant de supprimer ce noeud.</remove-popup-first>
      <confirm-delete-action>Ne yo voulez vraiment supprimer cette action?</confirm-delete-action>
      <template-empty>Modèle de '{0}' est nul ou vide</template-empty>
      <template-null>Modèle est nul ou vous n'avez pas la permission d'accéder à ce modèle.</template-null>
      <access-denied>Accès refusé! Vous n'avez pas la permission de le faire assez de cette action.</access-denied>

    </msg>
	</UIFastContentCreatorActionList>
	
	<!-- Select path popup -->
	<UIFastContentCreatorSelectorPopupWindow>
    <title>
      <UIOneNodePathSelector>Sélectionnez l'emplacement</UIOneNodePathSelector>
    </title>
  </UIFastContentCreatorSelectorPopupWindow>	
	
	<UIWorkspaceList>
    <label>
      <workspaceName>Workspace</workspaceName>
      <rootNodePath>Chemin d'accès racine</rootNodePath>
			<rootNodeInfo></rootNodeInfo>
    </label>
  </UIWorkspaceList>
	
	<rootNodeInfo>
		<title>
			<AddRootNode>Ajoutez root node</AddRootNode>
		</title>
	</rootNodeInfo>
	

  <UITreeList>
    <info>
      <child-not-found>Ce nœud n'a pas eu d'enfant</child-not-found>
    </info>
    <header>
      <name>Nom</name>
      <action>Action</action>
    </header>
  </UITreeList>
	
	<!-- Add action popup -->
	<UIFastContentCreatorActionPopupWindow>
		<title>
			<UIFastContentCreatorActionContainer>Ajouter la forme d'action</UIFastContentCreatorActionContainer>
		</title>
	</UIFastContentCreatorActionPopupWindow>
	
	<UIFastContentCreatorActionForm>
		<title>Ajouter / Éditer une action</title>
		<action>
			<Save>Sauver</Save>
			<Close>Fermer</Close>
		</action>
		<msg>
      <repository-exception>Ne peut pas enregistrer cette action. Le nom fourni ne doit pas avoir un indice sur son dernier élément.</repository-exception>
      <numberformat-exception>Ne peut pas enregistrer cette action. Le nom ne doit pas avoir fourni '[' ou ']'.</numberformat-exception>
      <existed-action>Le nom de l'action '{0}' est en vigueur, afin de choisir un autre nom, s'il vous plaît!</existed-action>
      <unable-add-action>Impossible d'ajouter cette action</unable-add-action>
      <unable-add>Impossible d'ajouter cette action! Ce nœud est la racine ou vous n'avez pas assez de permission</unable-add>
      <not-support>La boîte de dialogue '{0}' n'est pas supporté</not-support>
      <no-permission-add>Accès refusé! Vous n'avez pas le droit d'ajouter des mesures ici</no-permission-add>
      <access-denied>Accès refusé! Vous n'avez pas la permission d'accéder à ce modèle.</access-denied>
    </msg>
		<validator>
      <validator>Validator</validator>
      <destPath>En chemin</destPath>
    </validator>
	</UIFastContentCreatorActionForm>
		
	<UIFastContentCreatorActionTypeForm>
		<label>
			<actionType>Créer une action de type</actionType>
		</label>
		<msg>
  		<not-support>Impossible de charger le modèle de '{0}'.</not-support>
  		<do-not-permission>Vous n'avez pas la permission de charger modèle de '{0}'.</do-not-permission>
  	</msg>
	</UIFastContentCreatorActionTypeForm>
	
  <EmptyFieldValidator>
    <msg>
      <empty-input>Type de valeur sur le terrain '{0}' s'il vous plaît</empty-input>
    </msg>
  </EmptyFieldValidator>
	
	


	<!-- check -->
  <PopupComponent>
    <title>
      <UIOneNodePathSelector>Sélectionnez l'emplacement</UIOneNodePathSelector>
      <UIPermissionSelector>Permission Browser</UIPermissionSelector>
      <UICategoriesSelector>Ajouter Catégories</UICategoriesSelector>
    </title>
  </PopupComponent>
	
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Voulez-vous supprimer?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>





  <DateTimeValidator>
    <msg>
      <Invalid-input>Champ '{0}' avec la valeur '{1}' est invalide</Invalid-input>
    </msg>
  </DateTimeValidator>





  <ECMNameValidator>
    <msg>
      <Invalid-char>Il ya des caractères non valides dans le champ '{0}'. Un autre type de valeur, s'il vous plaît!</Invalid-char>

      <empty-input>La valeur du champ '{0}' est nul ou vide!</empty-input>
    </msg>
  </ECMNameValidator>
  











  <UITabPane>
    <title>
      <UICategoriesSelectPanel>Sélectionner les catégories</UICategoriesSelectPanel>
      <UISelectedCategoriesGrid>Referenced catégories</UISelectedCategoriesGrid>
    </title>
  </UITabPane>
  <PopupComponent>
    <title>
      <UICategoriesSelector>Ajouter Catégories</UICategoriesSelector>
    </title>
  </PopupComponent>
  <UICategoriesSelector>
    <action>
      <close>Fermer</close>
    </action>
  </UICategoriesSelector>

  <PopupComponent>
    <title>
      <UIOneTaxonomySelector>Ajouter Catégories</UIOneTaxonomySelector>
    </title>
  </PopupComponent>
  
  <UITreeTaxonomyList>
    <label>
      <taxonomyTree>Taxonomie Tree</taxonomyTree>
    </label>
  </UITreeTaxonomyList>
  
  






</bundle>

