<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--/n  #########################################################################################
  #			org.exoplatform.wcm.webui.search.UIWCMSearchForm							#
  #########################################################################################
  -->
  <UISearchForm>
    <keywordInput>
      <label>Keyword</label>
    </keywordInput>
    <portalSelector>
      <label>Portal</label>
    </portalSelector>
    <documentCheckBox>
      <label>Document</label>
    </documentCheckBox>
    <pageCheckBox>
      <label>Page</label>
    </pageCheckBox>
    <action>
      <Search>Search</Search>
    </action>
    <message>
      <keyword-not-empty>Keyword is not empty.</keyword-not-empty>
      <not-checked>You should check type of result which you want to search.</not-checked>
      <keyword-not-support>The search engine can not currently work with the keywords you supplied.</keyword-not-support>
    </message>
  </UISearchForm>
  <!--/n  #########################################################################################
  #			org.exoplatform.wcm.webui.search.config.UISearchPageLayoutManager			#
  #########################################################################################
  -->
  <UISearchPageLayoutManager>
    <mode>
      <selectOption>
        <label>---Select search mode---</label>
      </selectOption>
      <box-search>
        <label>Box search</label>
      </box-search>
      <page-search>
        <label>Page search</label>
      </page-search>
    </mode>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
    <label>
      <searchFormSelector>Search form templates</searchFormSelector>
      <searchPaginatorSelector>Search paginator templates</searchPaginatorSelector>
      <searchResultSelector>Search result templates</searchResultSelector>
      <searchBoxSelector>Search box templates</searchBoxSelector>
      <searchModeSelector>Search modes</searchModeSelector>
      <itemsPerPageSelector>Items per page</itemsPerPageSelector>
      <searchPageLayoutSelector>Search page layout template</searchPageLayoutSelector>
      <viewerButtonQuickEdit>Show quick edit</viewerButtonQuickEdit>
    </label>
    <message>
      <search-mode-selecting>You should select the search mode.</search-mode-selecting>
    </message>
  </UISearchPageLayoutManager>
  <!--/n  #########################################################################################
  #			org.exoplatform.wcm.webui.search.UISearchResult								#
  #########################################################################################
  -->
  <UISearchResult>
    <msg>
      <your-search>Your search</your-search>
      <did-not-match>did not match any</did-not-match>
      <suggestions>Suggestions:</suggestions>
      <try-different-key>Try different keywords.</try-different-key>
      <keyword_entered>Enter keyword for search.</keyword_entered>
      <new-suggestion>Did you mean</new-suggestion>
    </msg>
  </UISearchResult>
  <UIDocumentDialogForm>
  	<action>
  		<Cancel>Cancel</Cancel>
  		<FastPublish>Fast Publish</FastPublish>
  		<SaveAsDraft>Save as Draft</SaveAsDraft>  		  		
  	</action>
  </UIDocumentDialogForm>
  <UISearchedContentEdittingPopupWindow>
  	<title>
  		<UIContentEdittingPopup>Content editting</UIContentEdittingPopup>
  	</title>
  </UISearchedContentEdittingPopupWindow>
</bundle>