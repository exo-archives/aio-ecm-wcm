<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--/n  #########################################################################################
        
  #			org.exoplatform.wcm.webui.search.UIWCMSearchForm                          #
          #########################################################################################
  -->
  <UISearchForm>
    <keywordInput>
      <label>Rechercher</label>
    </keywordInput>
    <portalSelector>
      <label>Site</label>
    </portalSelector>
    <documentCheckBox>
      <label>Documents</label>
    </documentCheckBox>
    <pageCheckBox>
      <label>Pages</label>
    </pageCheckBox>
    <action>
      <Search>Rechercher</Search>
    </action>
    <message>
      <keyword-not-empty>Le mot n'est pas vide.</keyword-not-empty>
      <not-checked>Vous devez sélectionner un type de contenu à rechercher.</not-checked>
      <keyword-not-support>Le moteur de recherche ne peut actuellement fonctionner avec les mots spécifiés.</keyword-not-support>
      <get-property> erreur lorsque offres de biens nœud. </get-property>
      <node-publication-error> d'erreur lorsque vous effectuez obtenir noeud cycle de publication de vie. </node-publication-error>
    </message>
  </UISearchForm>
  <!--/n  #########################################################################################
        
  #			org.exoplatform.wcm.webui.search.config.UISearchPageLayoutManager         #
          #########################################################################################
  -->
  <UISearchPageLayoutManager>
    <mode>
      <selectOption>
        <label>---Mode de recherche----</label>
      </selectOption>
      <box-search>
        <label>Boîte</label>
      </box-search>
      <page-search>
        <label>Page</label>
      </page-search>
    </mode>
    <action>
      <Save>Sauver</Save>
      <Cancel>Annuler</Cancel>
    </action>
    <label>
      <searchFormSelector>Modèles de formulaires</searchFormSelector>
      <searchPaginatorSelector>Modèle de pagination</searchPaginatorSelector>
      <searchResultSelector>Modèles de résultats</searchResultSelector>
      <searchBoxSelector>Modèles pour les boîtes</searchBoxSelector>
      <searchModeSelector>Modes de recherche</searchModeSelector>
      <itemsPerPageSelector>Eléments par page</itemsPerPageSelector>
      <searchPageLayoutSelector>Disposition de la page de recherche</searchPageLayoutSelector>
      <viewerButtonQuickEdit>Edition rapide</viewerButtonQuickEdit>
    </label>
    <message>
      <search-mode-selecting>Vous devez sélectionner un mode de recherche.</search-mode-selecting>
    </message>
  </UISearchPageLayoutManager>
  <!--/n  #########################################################################################
        
  #			org.exoplatform.wcm.webui.search.UISearchResult                           #
          #########################################################################################
  -->
  <UISearchResult>
    <msg>
      <your-search>Votre recherche</your-search>
      <did-not-match>n'a délivré aucun </did-not-match>
      <suggestions>Suggestions:</suggestions>
      <try-different-key>Utiliser d'autres critères de recherche.</try-different-key>
      <keyword_entered>Insérer des mots à rechercher.</keyword_entered>
      <new-suggestion>Voulez-vous dire</new-suggestion>
    </msg>
  </UISearchResult>
  <UIDocumentDialogForm>
  	<action>
  		<Cancel></Cancel>
  		<FastPublish></FastPublish>
  		<SaveAsDraft></SaveAsDraft>  		  		
  	</action>
  </UIDocumentDialogForm>    
  <UISearchedContentEdittingPopupWindow>
  	<title>
  		<UIContentEdittingPopup>Contenu Editting</UIContentEdittingPopup>
  	</title>
  </UISearchedContentEdittingPopupWindow>
  <UIContentEdittingPopupWindow>
  	<title>
  		<UIDocumentDialogForm>Contenu Editting</UIDocumentDialogForm>
  	</title>
  </UIContentEdittingPopupWindow>
  <UIDocumentForm>
	<msg>
		<repository-exception>Permission d'accès refusée.</repository-exception>
	</msg>
  </UIDocumentForm>
  <UIPermissionSetting>
    <label>
      <usersInputSet>UIPermissionSetting</usersInputSet>
      <usersStringInput>Utilisateur ou groupe</usersStringInput>
      <editable>Droits d'édition</editable>
      <accessible>Droits de visualisation</accessible>
    </label>
    <action>
      <Save>Sauver</Save>
      <Reset>Réinitialiser</Reset>
    </action>
    <msg>
      <userOrGroup-required>Un utilisateur ou un groupe est requis.</userOrGroup-required>
      <checkbox-require>Un droit est requis.</checkbox-require>
    </msg>
  </UIPermissionSetting>
  <usersInputSet>
    <title>
      <SelectUser>Sélectionnez l'utilisateur</SelectUser>
      <SelectMember>Sélectionnez membre</SelectMember>
      <AddAny>Ajouter toute autorisation</AddAny>
    </title>
  </usersInputSet>
</bundle>