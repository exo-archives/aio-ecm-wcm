<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--/n  ####################################################################
        
  # org.exoplatform.wcm.webui.scv.config.UIWelcomeScreen             #
          ####################################################################
  -->
  <UIWelcomeScreen>
    <label>
      <QuickCreateWebContent>Création rapide de contenu web</QuickCreateWebContent>
      <SelectExistedWebContent>Sélectionner un contenu web existant</SelectExistedWebContent>
      <SelectExistedDMS>Sélectionner un contenu documentaire existant</SelectExistedDMS>
      <EditWebContent>Editer le contenu web courant</EditWebContent>
      <ExistedContent>Sélectionner un contenu existant</ExistedContent>
      <radio />
    </label>
    <action>
      <StartProcess>OK</StartProcess>
      <Back>Retour</Back>
    </action>
    <WelcomeLable>Bienvenue au visualiseur de contenu simples!</WelcomeLable>
    <Text>Veuillez sélectionner l'une des options et cliquer sur OK.</Text>
  </UIWelcomeScreen>
  <UITabPane>
    <title>
      <UITagging>Tags</UITagging>
      <UICategorizing>Catégories</UICategorizing>
      <UIMiscellaneousInfo>Divers</UIMiscellaneousInfo>
      <UIPermissionManager>Gestion des permissions</UIPermissionManager>
      <UINameWebContentForm>Disposition du contenu web</UINameWebContentForm>
      <UIWCMPublicationGrid>Cycle de publication</UIWCMPublicationGrid>
    </title>
  </UITabPane>
  <!--/n  ###################################################################
          # org.exoplatform.wcm.webui.scv.config.UIMiscellaneousInfo        #
          ###################################################################
  -->
  <UIMiscellaneousInfo>
    <label>
      <ShowTitle>Affichage du titre</ShowTitle>
      <ShowTags>Affichage des tags</ShowTags>
      <ShowCategories>Affichage des catégories</ShowCategories>
      <ShowQuickEdit>Affichage de l'icône d'édition rapide</ShowQuickEdit>
    </label>
    <action>
      <SaveToPortletPreference>Sauver</SaveToPortletPreference>
    </action>
  </UIMiscellaneousInfo>
  <!--/n  ###################################################################
          # org.exoplatform.wcm.webui.scv.config.UIQuickCreationWizard      #
          ###################################################################
  -->
  <UIQuickCreationWizard>
    <label>
      <stepInfo>Affichage des informations d'étapes</stepInfo>
      <contentCreateWizard>Assitant de création de contenu:</contentCreateWizard>
      <step1>
        <title>Sélection d'une disposition de contenu web et d'un cycle de publication</title>
        <describe>Créer un contenu web en spécifiant un nom et un résumé.</describe>
      </step1>
      <step2>
        <title>Création du contenu web</title>
        <describe>Créer un contenu web en spécifiant le code HTML, le style CSS, le code JavaScript.</describe>
      </step2>
      <step3>
        <title>Paramètres</title>
        <describe>Spécifier les paramètres d'affichage optionnels</describe>
      </step3>
      <wizardSteps>Etapes</wizardSteps>
      <wizardStep>Etape</wizardStep>
    </label>
    <action>
      <Abort>Annuler</Abort>
      <Back>Retour</Back>
      <ViewStep2>Suivant</ViewStep2>
      <ViewStep3>Suivant</ViewStep3>
      <Complete>Fin</Complete>
    </action>
    <msg>
      <StepbyStep>Pour la première fois, il est nécessaire de parcourir chacune des étapes.</StepbyStep>
    </msg>
  </UIQuickCreationWizard>
  <!--/n  ###################################################################
        
  # org.exoplatform.wcm.webui.scv.config.UIWebContentSelector       #
		  ###################################################################
  -->
  <UIWebContentSelector>
    <label>
      <repository>repository</repository>
      <workspace>workspace</workspace>
      <location>Emplacement</location>
      <Describe>Description</Describe>
      <path />
    </label>
    <action>
      <Save>Sauver</Save>
      <Back>Retour</Back>
    </action>
    <msg>
      <require-choose>Veuillez choisir un contenu web existant!</require-choose>
    </msg>
  </UIWebContentSelector>
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Etes-vous sûr de vouloir supprimer cette catégorie de la liste?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <!--/n  ###################################################################
          # org.exoplatform.wcm.webui.scv.config.UIContentDialogForm        #
          ###################################################################
  -->
  <UIContentDialogForm>
    <action>
      <Save>Sauver</Save>
      <Cancel>Annuler</Cancel>
    </action>
  </UIContentDialogForm>
  <!--/n  ###################################################################
        
  # org.exoplatform.wcm.presentation.scp.UIMessageBoa               #
          ###################################################################
  -->
  <UIMessageBoard>
    <msg>
      <content-not-found>Contenu non trouvé ou cet emplacement n'a pas été configuré!</content-not-found>
      <no-permission>Vous n'avez pas les permissions requises pour afficher le contenu!</no-permission>
      <error-nodetype>Ce visualiseur n'a pas été configuré.</error-nodetype>
    </msg>
  </UIMessageBoard>
  <!--/n  ###################################################################
          # org.exoplatform.dms.webui.component.UICategoriesAddedList       #
          ###################################################################
  -->
  <UICategoriesAddedList>
    <msg>
      <confirm-delete>Etes-vous sûr de supprimer cette catégorie de la liste?</confirm-delete>
    </msg>
    <header>
      <path>Chemin</path>
      <action>Supprimer</action>
    </header>
  </UICategoriesAddedList>
  <!--/n  ###################################################################
          # org.exoplatform.dms.webui.component.UICategoryManager           #
          ###################################################################
  -->
  <UICategoryManager>
    <tab>
      <label>
        <UICategoriesAddedList>Lister les catégories</UICategoriesAddedList>
        <UINodesExplorer>Explorateur de catégories</UINodesExplorer>
      </label>
    </tab>
  </UICategoryManager>
  <!--/n  ###################################################################
          # org.exoplatform.ecm.webui.component.UITreeLis                   #
          ###################################################################
  -->
  <UITreeList>
    <header>
      <name>Nom</name>
      <action>Ajouter</action>
    </header>
    <info>
      <child-not-found>Aucun fils trouvé!</child-not-found>
    </info>
  </UITreeList>
  <!--/n  ###################################################################
  -->
  <UIViewModeContainer>
    <action>
      <QuickEdit>Editer</QuickEdit>
    </action>
  </UIViewModeContainer>
  <PopupWebContentSelector>
    <title>
      <UIWebContentPathSelector>Sélectionner un contenu web</UIWebContentPathSelector>
    </title>
  </PopupWebContentSelector>
  <PopupDMSSelector>
    <title>
      <UIDocumentPathSelector>Sélectionner un contenu documentaire</UIDocumentPathSelector>
    </title>
  </PopupDMSSelector>
  <UIPopupWindow>
    <title>
      <UIPortletConfig>Visualiseur de contenu simple</UIPortletConfig>
    </title>
  </UIPopupWindow>
  <SelectUserOrGroup>
    <title>
      <UIGroupMemberSelector>Sélecteur de membership</UIGroupMemberSelector>
      <UIWCMUserContainer>Sélecteur d'utilisateur</UIWCMUserContainer>
    </title>
  </SelectUserOrGroup>
  <UIGroupMemberSelector>
    <label>
      <selectGroup>Sélecteur de groupe</selectGroup>
      <selectMembership>Sélecteur de membership</selectMembership>
    </label>
  </UIGroupMemberSelector>
  <usersInputSet>
    <title>
      <AddAny>Ajouter les permissions à tout le monde</AddAny>
    </title>
  </usersInputSet>
  <UIPermissionInfo>
    <action>
      <title>
        <Edit>Editer</Edit>
        <Delete>Supprimer</Delete>
      </title>
    </action>
    <header>
      <usersOrGroups>Utilisateur ou groupe</usersOrGroups>
      <accessible>Accessible</accessible>
      <editable>Editable</editable>
      <action>Action</action>
    </header>
    <msg>
      <confirm-delete-permission>Confirmez-vous vouloir supprimer cette permission ?</confirm-delete-permission>
      <no-permission-remove>Il n'y a pas de permission à supprimer</no-permission-remove>
      <access-denied>Accès refusé</access-denied>
    </msg>
  </UIPermissionInfo>
  <location>
    <title>
      <Browse />
    </title>
  </location>
  <publication>
    <title>
      <BrowsePublication />
    </title>
  </publication>
  <!--/n  ##################################################################################
          # org.exoplatform.wcm.presentation.acp.config.selector.UIWebContentPathSelector  #
          ##################################################################################
  -->
  <UIWebContentSelectorForm>
    <title>Sélectionner un contenu web et le publier sur cette page</title>
    <label>
      <path />
      <publication>Publier par</publication>
      <PublicationPath />
    </label>
    <action>
      <Save>Sauver</Save>
      <Back>Retour</Back>
    </action>
  </UIWebContentSelectorForm>
  <!--/n  ##################################################################################
  -->
  <UIWizardWelcome>
    <WelcomeLabel>Bienvenue à l'assitant de création de contenu web !</WelcomeLabel>
    <WelcomeText>Cet assistant va vous accompagner durant la création de contenu web. Il va vous guider pour soumettre les informations nécessaire à la création du contenu. Quand vous aurez terminé d'entrer les informations requises, vous aurez l'occasion de sauver votre nouveau contenu web. Vous aurez aussi l'occasion de naviguer dans l'assistant en utilisant les boutons "Retour" et "Suivant".</WelcomeText>
  </UIWizardWelcome>
  <!--/n  ######################################################################
          # org.exoplatform.wcm.presentation.acp.config.UITagging              #
          ######################################################################
  -->
  <UITagging>
    <label>
      <TagNames>Noms des tags</TagNames>
      <LinkedTags>Tags</LinkedTags>
    </label>
    <action>
      <AddTags>Ajouter des tags</AddTags>
    </action>
    <msg>
      <tag-name-empty>Le nom du tag est vide</tag-name-empty>
      <name-exist>Le nom du tag existe déjà</name-exist>
      <tagName-invalid>Il y a des caractères invalides dans le nom du tag.</tagName-invalid>
      <tagName-too-long>Un nom de tag est trop long. La longueur des noms de tags ne doit pas excéder 20 caractères.</tagName-too-long>
    </msg>
    <introduction>
      <tagName>(Utiliser le séparateur "," pour ajouter simultanément plusieurs tags)</tagName>
    </introduction>
  </UITagging>
  <!--/n  ##############################################################################
          # org.exoplatform.wcm.presentation.acp.config.UIPermissionSetting            #
          ##############################################################################
  -->
  <UIPermissionSetting>
    <label>
      <usersInputSet>UIPermissionSetting</usersInputSet>
      <usersStringInput>Utilisateur ou groupe</usersStringInput>
      <editable>Droits d'édition</editable>
      <accessible>Droits de visualisation</accessible>
    </label>
    <action>
      <Save>Sauver</Save>
      <Reset>Réinitialiser</Reset>
    </action>
    <msg>
      <userOrGroup-required>Un utilisateur ou un groupe est requis.</userOrGroup-required>
      <checkbox-require>Un droit est requis.</checkbox-require>
    </msg>
  </UIPermissionSetting>
  <!--/n  ##############################################################################
          # org.exoplatform.wcm.presentation.acp.config.UIPermissionInfo               #
          ##############################################################################
  -->
  <!--/n  #############################################################
  -->
  <UIDocumentForm>
    <msg>
      <not-support>Non supporté</not-support>
    </msg>
  </UIDocumentForm>
  <!--/n  #############################################################
  -->
  <UIPresentationContainer>
    <title>Edition rapide</title>
  </UIPresentationContainer>
  <!--/n  #############################################################
        
  # org.exoplatform.wcm.webui.scv.config.UIDMSSelectorForm    #
          #############################################################
  -->
  <UIDMSSelectorForm>
    <label>
      <path />
      <publication>Publier par</publication>
      <PublicationPath />
    </label>
    <action>
      <Save>Sauver</Save>
      <Back>Retour</Back>
    </action>
    <msg>
      <require-choose>Veuillez s'il vous plaît sélectionner un contenu documentaire!</require-choose>
    </msg>
    <title>Sélectionner un document et le publier sur une page</title>
  </UIDMSSelectorForm>
  <!--/n  ##############################################################
  -->
  <UINameWebContentForm>
    <label>
      <name>Nom</name>
      <selectTemplate>Disposition</selectTemplate>
    </label>
    <action>
      <Save>Sauver</Save>
      <Abort>Annuler</Abort>
    </action>
    <msg>
      <non-lifecyclename>Veuillez sélectionner un nom de cycle de vie!</non-lifecyclename>
      <non-firstwhiteletter>Impossible de mettre un espace en premier caractère du nom!</non-firstwhiteletter>
    </msg>
  </UINameWebContentForm>
  <!--/n  ##########################################################################
  -->
  <UINonEditable>
    <action>
      <BackToView>Retour en mode visualisation</BackToView>
    </action>
  </UINonEditable>
  <UIQuickEditContainer>
    <action>
      <BackToNormalWizard>Retour</BackToNormalWizard>
    </action>
    <back>Cliquer ici pour accéder aux options avancées de configuration</back>
  </UIQuickEditContainer>
  <UIPopupMenu>
    <msg>
      <node-locked>L'élément de chemin {0} ou l'un de ses parents est verrouillé.</node-locked>
    </msg>
  </UIPopupMenu>
  <!--/n  ###########################################################################
  -->
  <UIWCMPublicationGrid>
    <header>
      <LifecycleName>Nom de cycle de publication</LifecycleName>
      <LifecycleDesc>Description du cycle de publication</LifecycleDesc>
      <action>Sélectionner</action>
    </header>
    <non-lifecyclename>Aucun cycle de vie est sélectionné</non-lifecyclename>
    <lifecyclename-selected>Ce contenu sera publié avec le cycle de publication suivant:</lifecyclename-selected>
  </UIWCMPublicationGrid>
  <UIQuickEditWebContentForm>
    <action>
      <Save>Sauver</Save>
      <Cancel>Annuler</Cancel>
    </action>
  </UIQuickEditWebContentForm>
  <UIStartEditionInPageWizard>
    <link>Cliquer ici pour accéder au dialogue d'édition du contenu!</link>
  </UIStartEditionInPageWizard>
</bundle>

