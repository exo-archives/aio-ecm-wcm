<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<UINesletterPopupWindow>
		<title>
			<UICategoryForm>
				Category Form
			</UICategoryForm>
			<UIManagerUsers>
				Manager Users
			</UIManagerUsers>
		</title>
	</UINesletterPopupWindow>
  <NewsletterPortletAction>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
      <Down>Down</Down>
      <Up>Up</Up>      
      <Delete>Delete</Delete>
      <Moderation>Moderation</Moderation>
      <CreateLetter>Create a new letter</CreateLetter>  
    </action>
  </NewsletterPortletAction>
  <UICategoryForm>
    <msg>
      <saveSuccess>Category has been saved.</saveSuccess>
      <categoryNameIsNotEmpty>Category name cannot be empty!</categoryNameIsNotEmpty>
      <categoryTitleIsNotEmpty>Category title cannot be empty!</categoryTitleIsNotEmpty>
      <categoryNameIsAlreadyExist>This category's name is already exist!</categoryNameIsAlreadyExist>
    </msg>
    <title>
    	<CategoryName>Category Name</CategoryName>
    	<CategoryTitle>Category Title</CategoryTitle>
    	<CategoryDescription>Description</CategoryDescription>
    	<CategoryModerator>Moderator </CategoryModerator>
    </title>
  </UICategoryForm>
  <UICategories>
    <title>
    	<SubScription>Subscription</SubScription>
    	<NumberOfUser>Number of User</NumberOfUser>
    	<AWaitingLetter>Awaiting Letter</AWaitingLetter>
    </title>
  </UICategories>
  <UISubcriptionForm>
    <msg>
      <saveSuccess>Subcription has been saved.</saveSuccess>
      <subcriptionNameIsNotEmpty>Subcription name cannot be empty!</subcriptionNameIsNotEmpty>
      <subcriptionTitleIsNotEmpty>Subcription title cannot be empty!</subcriptionTitleIsNotEmpty>
      <subcriptionNameIsAlreadyExist>This subcription's name is already exist!</subcriptionNameIsAlreadyExist>
    </msg>
    <title>
    	<SubcriptionName>Subcription Name</SubcriptionName>
    	<SubcriptionTitle>Subcription Title</SubcriptionTitle>
    	<SubcriptionDescription>Description</SubcriptionDescription>
    	<CategoryName>Category Name </CategoryName>
    </title>
  </UISubcriptionForm>
  <UISubscription>
    <msg>
    	<confirmDeleteCategory>Are you sure you want to delete this category?</confirmDeleteCategory>
    	<confirmDeleteSubscription>Are you sure you want to delete this subscription</confirmDeleteSubscription>
    	<checkOnlyOneSubScriptionToEdit>Please check only one subscription to edit!</checkOnlyOneSubScriptionToEdit>
    	<checkOnlyOneSubScriptionToDelete>Please check only one subscription to delete!</checkOnlyOneSubScriptionToDelete>
    	<checkOnlyOneSubScriptionToOpen>Please check only one subscription to open!</checkOnlyOneSubScriptionToOpen>
    </msg>
  </UISubscription>
</bundle>