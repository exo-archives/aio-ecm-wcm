<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<UICategoryFormPopupWindow>
		<title>
			<UICategoryForm>Category form</UICategoryForm>
		</title>
	</UICategoryFormPopupWindow>
	<UISubscriptionFormPopupWindow>
