<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<NewsletterPortlet>
    <action>
    	<Preferences>Preferences</Preferences>
      <Save>Save</Save>
      <Open>Open</Open>
      <Cancel>Cancel</Cancel>
      <Close>Close</Close>
      <Down>Down</Down>
      <Up>Up</Up>      
      <Delete>Delete</Delete>
      <Edit>Edit</Edit>
      <Moderation>Moderation</Moderation>
      <CreateLetter>New Letter</CreateLetter>  
      <UpdateNewsletter>Update Newsletter's infor</UpdateNewsletter>
    </action>
    <title>
    	<eXoNewsletter>eXo Newsletter</eXoNewsletter>
    	<Preferences>Preferences</Preferences>
    </title>
  </NewsletterPortlet>
  
	<UITabPane>
		<title>
			<UIManagerUsers>Public Users</UIManagerUsers>
			<UIManagerModerator>Users Manager</UIManagerModerator>
		</title>
	</UITabPane>
	
	<UIWebcontentSelectorFormPopupWindow>
		<title>
			<UINewsletterEntryWebcontentSelectorForm>Webcontent selector form</UINewsletterEntryWebcontentSelectorForm>
		</title>
	</UIWebcontentSelectorFormPopupWindow>
	
	<UIWebcontentSelectorPopupWindow>
		<title>
			<UIWebContentTabSelector>Webcontent selector</UIWebContentTabSelector>
		</title>
	</UIWebcontentSelectorPopupWindow>
	
	<UINewsletterEntryWebcontentSelectorForm>
		<label>
			<WebcontentSelector></WebcontentSelector>
		</label>
	</UINewsletterEntryWebcontentSelectorForm>
	
	<FormWebcontentSelector>
		<title>
			<SelectWebcontent>Select webcontent</SelectWebcontent>
		</title>
	</FormWebcontentSelector>
	
	<UICategoryFormPopupWindow>
		<title>
			<UICategoryForm>Category form</UICategoryForm>
		</title>
	</UICategoryFormPopupWindow>
	
	<UISubscriptionFormPopupWindow>
		<title>
			<UISubcriptionForm>Subscription form</UISubcriptionForm>
			<UIManagerUsers>Manager Users</UIManagerUsers>
		</title>
	</UISubscriptionFormPopupWindow>
	
	<UINewsletterEntryFormPopupWindow>
		<title>
			<UINewsletterEntryContainer>Newsletter entry form</UINewsletterEntryContainer>
		</title>
		<action>
			<Cancel>Cancel</Cancel>
		</action>
	</UINewsletterEntryFormPopupWindow>
	
	<UIManagerUsersFormPopupWindow>
		<title>
			<UIManagerUsers>Manager Users</UIManagerUsers>
		</title>
	</UIManagerUsersFormPopupWindow>
	
  <UICategoryForm>
    <msg>
      <saveSuccess>Category has been saved.</saveSuccess>
      <categoryNameIsNotEmpty>Category name cannot be empty!</categoryNameIsNotEmpty>
      <categoryTitleIsNotEmpty>Category title cannot be empty!</categoryTitleIsNotEmpty>
      <categoryNameIsAlreadyExist>This category's name is already exist!</categoryNameIsAlreadyExist>
      <inputModeratorEmpty>You must select user or member moderator!</inputModeratorEmpty>
    </msg>
		<label>
    	<CategoryName>Category Name</CategoryName>
    	<CategoryTitle>Category Title</CategoryTitle>
    	<CategoryDescription>Category Description</CategoryDescription>
			<FormCategoryModerator>Category moderator</FormCategoryModerator>
			<CategoryModerator></CategoryModerator>
		</label>
		<action>
			<Save>Save</Save>
			<Cancel>Cancel</Cancel>
		</action>
  </UICategoryForm>
  
  <UICategories>
    <title>
    	<SubScription>Subscription</SubScription>
    	<NumberOfUser>Number of User</NumberOfUser>
    	<AWaitingLetter>Awaiting Letter</AWaitingLetter>
    	<NewsLetter>New Letter</NewsLetter>
    	<NewsSubscription>New Subscription</NewsSubscription>
    	<NewsCategory>New Category</NewsCategory>
    	<Administration>Administration</Administration>
    </title>
  </UICategories>
  
  <UISubscription>
    <msg>
    	<confirmDeleteCategory>Are you sure you want to delete this category ?</confirmDeleteCategory>
    	<confirmDeleteSubscription>Are you sure you want to delete this subscription ?</confirmDeleteSubscription>
    	<checkOnlyOneSubScriptionToEdit>Please check only one subscription to edit!</checkOnlyOneSubScriptionToEdit>
    	<checkOnlyOneSubScriptionToDelete>Please check only one subscription to delete!</checkOnlyOneSubScriptionToDelete>
    	<subscriptionNotfound>Subcription name doesn't exist!</subscriptionNotfound>
    	<checkOnlyOneSubScriptionToOpen>Please check only one subscription to open!</checkOnlyOneSubScriptionToOpen>
    </msg>
    <title>
    	<ManageCategory>Manage Category</ManageCategory>
    	<EditCategory>Edit Category</EditCategory>
    	<DeleteCategory>Delete Category</DeleteCategory>
    	<Edit>Edit</Edit>
    	<Delete>Delete</Delete>
    	<ManageUsers>Manage Users</ManageUsers>
    	<Open>Open</Open>
    </title>
  </UISubscription>
	
	<FormCategoryModerator>
		<title>
			<SelectUser>Select user</SelectUser>
			<SelectMember>Select member</SelectMember>
		</title>
	</FormCategoryModerator>
	
	<UIGroupMemberSelector>
		<label>
			<selectGroup>Select group</selectGroup>
			<selectMembership>Select membership</selectMembership>
		</label>
	</UIGroupMemberSelector>
	
	<UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Are you sure to delete this category from list?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  
	<UIUserMemberSelectorPopupWindow>
		<title>
			<UIUserMemberSelector>User selector</UIUserMemberSelector>
		</title>
	</UIUserMemberSelectorPopupWindow>
	
	<UIGroupMemberSelectorPopupWindow>
		<title>
			<UIGroupMemberSelector>Group selector</UIGroupMemberSelector>
		</title>
	</UIGroupMemberSelectorPopupWindow>
	
  <UISubcriptionForm>
    <msg>
      <saveSuccess>Subcription has been saved.</saveSuccess>
      <subcriptionNameIsNotEmpty>Subcription name cannot be empty!</subcriptionNameIsNotEmpty>
      <subcriptionTitleIsNotEmpty>Subcription title cannot be empty!</subcriptionTitleIsNotEmpty>
      <subcriptionNameIsAlreadyExist>This subcription's name is already exist!</subcriptionNameIsAlreadyExist>
    </msg>
    <title>
    	<SubcriptionName>Subcription Name</SubcriptionName>
    	<SubcriptionTitle>Subcription Title</SubcriptionTitle>
    	<SubcriptionDescription>Description</SubcriptionDescription>
    	<CategoryName>Category</CategoryName>
    </title>
  </UISubcriptionForm>
	
	<UINewsletterEntryForm>
		<action>
			<Select>Select</Select>
			<Preview>Preview</Preview>
			<Save>Save</Save>
			<Send>Send</Send>
			<Cancel>Cancel</Cancel>
		</action>
		<msg>
			<UpdateBeforeSave>
				Please click to update newsletter's infor before send or save! 
			</UpdateBeforeSave>
		</msg>
	</UINewsletterEntryForm>
	
	<UINewsletterEntryDialogSelector>
		<action>
			<OpenWebcontentSelectorForm>Open webcontent selector</OpenWebcontentSelectorForm>
		</action>
		<title>
			<UINewsletterEntryTemplate>Template</UINewsletterEntryTemplate>
			<UINewsletterEntrySendDate>Send date</UINewsletterEntrySendDate>
			<UINewsletterEntryCategorySelectBox>Category</UINewsletterEntryCategorySelectBox>
			<UINetterEntrySubscriptionSelectBox>Subscription</UINetterEntrySubscriptionSelectBox>
			<UINewsletterEntrySubscriptionSelectBox>Subscription</UINewsletterEntrySubscriptionSelectBox>
		</title>
	</UINewsletterEntryDialogSelector>
	
	<UIManagerUsers>
		<header>
			<Mail>Email</Mail>
			<isBanned>Banned</isBanned>
			<action> </action>
			<UserName>User Name</UserName>
			<FirstName>First Name</FirstName>
			<LastName>Last Name</LastName>
			<Email>Email</Email>
			<Role>Role</Role>
		</header>
		<action>
			<title>
				<Edit>Edit</Edit>
				<BanOrUnBanUser>Ban/Unban User</BanOrUnBanUser>
				<DeleteUser>Delete User</DeleteUser>
				<Addministrator>Administrator</Addministrator>
				<AddAdministrator>Set is Administrator of Newsletter</AddAdministrator>
				<DeleteAdministrator>Delete Administrator</DeleteAdministrator>
			</title>
		</action>
		<role>
			<Administrator>Administrator</Administrator>
			<Moderator>Moderator</Moderator>
			<Redactor>Redactor</Redactor>
		</role>
	</UIManagerUsers>
	
	<UINewsletterEntryManager>
		<msg>
			<confirmDeleteNewsletter>Are you sure you want to delete this newsletter?</confirmDeleteNewsletter>
		</msg>
		<title>
			<ConvertAsTemplate>Convert As Template</ConvertAsTemplate>
			<Letter>Letter</Letter>
			<Date>Date</Date>
			<Status>Status</Status>
		</title>
	</UINewsletterEntryManager>

	<UINewsletterEntryViewForm>
		<title>
			<UINewsletterManagerPopup>View Newsletter's Content</UINewsletterManagerPopup>
		</title>
	</UINewsletterEntryViewForm>
	
	<WebContent>
		<dialog>
			<label>
				<Advanced>Advanced</Advanced>
			</label>
		</dialog>
	</WebContent>	
</bundle>