<?xml version="1.0" encoding="UTF-8"?>
<bundle>
 <UIPresentationContainer>
  <title>
   <QuickEdit>Éditer rapidement</QuickEdit>
   <print>Imprimer cette page</print>
  </title>
  <label>
   <draft>Brouillon</draft>
   <print>Imprimer</print>
  </label>
  <msg>
   <no-content>Il n'y a pas de contenu à afficher</no-content>
  </msg>
 </UIPresentationContainer>

 <UIWelcomeScreen>
  <label>
   <WelcomeLabel>Bienvenue dans Single Content Viewer</WelcomeLabel>
   <WelcomeText>Faites votre choix</WelcomeText>
  </label>
  <action>
   <Abort>Annuler</Abort>
   <CreateNewContent>Créer un nouveau contenu</CreateNewContent>
   <SelectContent>Sélectionnez un contenu</SelectContent>
  </action>
 </UIWelcomeScreen>

 <UINameWebContentForm>
  <action>
   <Save>Enregistrer</Save>
   <Abort>Annuler</Abort>
  </action>
  <label>
   <name>Titre</name>
   <selectTemplate>Sélectionnez un modèle</selectTemplate>
   <option>
    <exo:siteBreadcrumb>Fil d'Ariane</exo:siteBreadcrumb>
    <exo:siteNavigationWebContent>Barre de Navigation</exo:siteNavigationWebContent>
    <exo:pictureOnHeadWebcontent>Contenu web contenant une image dans l'entête</exo:pictureOnHeadWebcontent>
    <exo:siteSearchBox>Formulaire de recherche dans le site</exo:siteSearchBox>
    <exo:webContent>Contenu web divers</exo:webContent>
   </option>
  </label>
 </UINameWebContentForm>

 <UITabPane>
   <title>
    <UIContentDialogConfig>Configuration pour Single content viewer</UIContentDialogConfig>
   </title>
  </UITabPane>
  
  <UIContentDialogConfig>
   <action>
    <SaveToPortletPreference>Enregistrer</SaveToPortletPreference>
   </action>
   <label>
    <ShowTitle>Afficher le titre</ShowTitle>
    <ShowPrintAction>Afficher l'imprimer</ShowPrintAction>
   </label>
   <msg>
    <saved-successfully>Vos préférences sont enregistrées</saved-successfully>
   </msg>
  </UIContentDialogConfig>
  
  <UIContentDialogFormPopupWindow>
   <title>
    <UIWelcomeScreen>Bienvenue dans Single content viewer</UIWelcomeScreen>
    <UINameWebContentForm>Suivant un modèle</UINameWebContentForm>
    <UIContentDialogForm>Formulaire du contenu web</UIContentDialogForm>
    <UIContentSelectorOne>Single content selector panel</UIContentSelectorOne>
   </title>
  </UIContentDialogFormPopupWindow>
 <Comments>
 	<tooltip>
		<edit-comment>Editer le commentaire</edit-comment>
		<remove-comment>Supprimer le commentaire</remove-comment>
 	</tooltip>
  </Comments>
</bundle>
