<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--/n  ######################################################################################
  #    org.exoplatform.ecm.webui.contentviewer.UIParameterizedContentViewerPortlet     #
  ######################################################################################
  -->
  <UIMessageBoard>
    <msg>
      <content-not-found>Content not found or deleted.</content-not-found>
      <content-unsupport-exc>Content not support to view.</content-unsupport-exc>
      <content-invisible>Content is invisible</content-invisible>
      <access-control-exc>Content access denied.</access-control-exc>
      <content-not-printed>Content cannot be printed</content-not-printed>
      <content-obsolete>Content is obsolete</content-obsolete>
      <get-node-publication>Error when get node publication lyfe cycle.</get-node-publication>
      <get-template-resource>Error when perform get template resource resolver.</get-template-resource>
      <get-node>Error when perform get child node.</get-node>
    </msg>
  </UIMessageBoard>
  <UIDocumentDialogForm>
  	<action>
  		<SaveDraft>Save Draft</SaveDraft>
  		<Cancel>Close</Cancel>
  		<FastPublish>Fast Publish</FastPublish>
  	</action>
  </UIDocumentDialogForm>
  <UIPCVContentDialog>
  	<action>
  		<SaveDraft>Save Draft</SaveDraft>
  		<Cancel>Close</Cancel>
  		<FastPublish>Fast Publish</FastPublish>
  	</action>
  </UIPCVContentDialog>
  <UIContentViewerContainer>
    <action>
      <back>Back</back>
      <print>Print</print>
      <close>Close</close>
    </action>
    <label>
    	<quickEditTitle>Quick Edit</quickEditTitle>
    </label>    
    <quickPrint>
    	<title>Print this page</title>
    	<print>Print this page</print>
    </quickPrint>
  </UIContentViewerContainer>
  <UIContentEdittingPopupWindow>
  	<title>
	  	<UIDocumentDialogForm>Document dialog form</UIDocumentDialogForm>
  	</title>
  </UIContentEdittingPopupWindow>
  <UIPresentationContainer>
  	<draft>Draft</draft>
  	<edit>Edit</edit>
  </UIPresentationContainer>
  <UIDocumentFormPopupWindow>
  	<title>
  		<UIPCVContentDialog>UI Parameter Content Viewer</UIPCVContentDialog>
  	</title>
  </UIDocumentFormPopupWindow>
  
  <UIPCVConfig>
  	<title>Parameterized Content Viewer Config</title>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
      <Down>Down</Down>
      <Up>Up</Up>      
      <Delete>Delete</Delete>            
    </action>
   	<msg>
   		<saving-success>
   			Parameter Content Viewer has been updated
   		</saving-success>
   		<not-a-number>
   			Item per page is not a number or value is not valid. Please input a valid number.
   		</not-a-number>
   		<decode>Unsupport encoding.</decode>
   		<get-illustration-image>Error when get illustration image.</get-illustration-image>
   		<get-node-publication>Error when get node publication lifecycle.</get-node-publication>
   	</msg>
    <label>
      <DisplaySettingBlock>Display setting</DisplaySettingBlock>    
      <ViewerTitle>Show title</ViewerTitle>
      <ViewerDateCreated>Show date created</ViewerDateCreated>
    </label>
  </UIPCVConfig>
</bundle>

