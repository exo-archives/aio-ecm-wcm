<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--/n  ####################################################################
  # org.exoplatform.wcm.webui.scv.config.UIWelcomeScreen             #
  ####################################################################
  -->
  <UIWelcomeScreen>
    <label>
      <QuickCreateWebContent>Quick web content creation</QuickCreateWebContent>
      <SelectExistedWebContent>Select existed web content</SelectExistedWebContent>
      <SelectExistedDMS>Select existed DMS document</SelectExistedDMS>
      <EditWebContent>Edit current web content</EditWebContent>
      <ExistedContent>Select an existed content</ExistedContent>
      <radio />
    </label>
    <action>
      <StartProcess>Ok</StartProcess>
      <Back>Back</Back>
    </action>
    <WelcomeLable>Welcome you to Single Content Viewer!</WelcomeLable>
    <Text>Please you check Edit this content or Select other content and click Ok button</Text>
  </UIWelcomeScreen>
  <UITabPane>
    <title>
      <UITagging>Tagging</UITagging>
      <UICategorizing>Categorizing</UICategorizing>
      <UIMiscellaneousInfo>Misc</UIMiscellaneousInfo>
      <UIPermissionManager>Permission setting</UIPermissionManager>
      <UINameWebContentForm>Webcontent layout</UINameWebContentForm>
      <UIWCMPublicationGrid>Publication lifecycle</UIWCMPublicationGrid>
    </title>
  </UITabPane>
  <!--/n  ###################################################################
  # org.exoplatform.wcm.webui.scv.config.UIMiscellaneousInfo        #
  ###################################################################
  -->
  <UIMiscellaneousInfo>
    <label>
      <ShowTOC>Show table of content</ShowTOC>
      <ShowTitle>Show title</ShowTitle>
      <ShowTags>Show tags</ShowTags>
      <ShowCategories>Show related category</ShowCategories>
      <AllowVoting>Show votes</AllowVoting>
      <AllowComment>Show comments</AllowComment>
      <ShowQuickEdit>Show quick edit</ShowQuickEdit>
    </label>
    <action>
      <SaveToPortletPreference>Save</SaveToPortletPreference>
    </action>
    <msg-successfully>You saved successfully!</msg-successfully>
  </UIMiscellaneousInfo>
  <!--/n  ###################################################################
  # org.exoplatform.wcm.webui.scv.config.UIQuickCreationWizard      #
  ###################################################################
  -->
  <UIQuickCreationWizard>
    <label>
      <stepInfo>Show step info</stepInfo>
      <contentCreateWizard>Create Content Wizard:</contentCreateWizard>
      <step1>
        <title>Select webcontent layout and publication lifecycle to publish</title>
        <describe>Create your web content by filling name, summary data into web content form.</describe>
      </step1>
      <step2>
        <title>Create web content</title>
        <describe>Create your web content by filling html, css, javascript data into web content form.</describe>
      </step2>
      <step3>
        <title>Settings</title>
        <describe>Socialize your web content by tagging, categorizing the content.</describe>
      </step3>
      <wizardSteps>Steps</wizardSteps>
      <wizardStep>Step</wizardStep>
    </label>
    <action>
      <Abort>Abort</Abort>
      <Back>Back</Back>
      <ViewStep2>Next</ViewStep2>
      <ViewStep3>Next</ViewStep3>
      <Complete>Finish</Complete>
    </action>
    <msg>
      <StepbyStep>In the first time, you must go through step by step</StepbyStep>
    </msg>
  </UIQuickCreationWizard>
  <!--/n  ###################################################################
  # org.exoplatform.wcm.webui.scv.config.UIWebContentSelector       #
  ###################################################################
  -->
  <UIWebContentSelector>
    <label>
      <repository>repository</repository>
      <workspace>workspace</workspace>
      <location>location</location>
      <Describe>Describe</Describe>
      <path />
    </label>
    <action>
      <Save>Save</Save>
      <Back>Back</Back>
    </action>
    <msg>
      <require-choose>Please choose an existing web content!</require-choose>
    </msg>
  </UIWebContentSelector>
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Are you sure to delete this category from list?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <!--/n  ###################################################################
  # org.exoplatform.wcm.webui.scv.config.UIContentDialogForm        #
  ###################################################################
  -->
  <UIContentDialogForm>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
  </UIContentDialogForm>
  <!--/n  ###################################################################
  #  org.exoplatform.wcm.presentation.scp.UIMessageBoard						#
  ###################################################################
  -->
  <UIMessageBoard>
    <msg>
      <content-not-found>Content not found or this portlet has not been configured yet!</content-not-found>
      <no-permission>You do not have permission to access the content!</no-permission>
      <error-nodetype>This viewer has not been configured yet.</error-nodetype>
    </msg>
  </UIMessageBoard>
  <!--/n  ###################################################################
  #  org.exoplatform.dms.webui.component.UICategoriesAddedList			#
  ###################################################################
  -->
  <UICategoriesAddedList>
    <msg>
      <confirm-delete>Are you sure to delete this category from list?</confirm-delete>
    </msg>
    <header>
      <path>Path</path>
      <action>Delete</action>
    </header>
  </UICategoriesAddedList>
  <!--/n  ###################################################################
  #  org.exoplatform.dms.webui.component.UICategoryManager					#
  ###################################################################
  -->
  <UICategoryManager>
    <tab>
      <label>
        <UICategoriesAddedList>List Categories</UICategoriesAddedList>
        <UINodesExplorer>Categories Explorer</UINodesExplorer>
      </label>
    </tab>
  </UICategoryManager>
  <!--/n  ###################################################################
  #  org.exoplatform.ecm.webui.component.UITreeList									#
  ###################################################################
  -->
  <UITreeList>
    <header>
      <name>Name</name>
      <action>Add</action>
    </header>
    <info>
      <child-not-found>Child not found!</child-not-found>
    </info>
  </UITreeList>
  <!--/n  ###################################################################
  -->
  <UIViewModeContainer>
    <action>
      <QuickEdit>Edit</QuickEdit>
    </action>
  </UIViewModeContainer>
  <PopupWebContentSelector>
    <title>
      <UIWebContentPathSelector>Select a webcontent</UIWebContentPathSelector>
    </title>
  </PopupWebContentSelector>
  <PopupDMSSelector>
    <title>
      <UIDocumentPathSelector>Select a DMS document</UIDocumentPathSelector>
    </title>
  </PopupDMSSelector>
  <UIPopupWindow>
    <title>
      <UIPortletConfig>Single Content Viewer</UIPortletConfig>
    </title>
  </UIPopupWindow>
  <SelectUserOrGroup>
    <title>
      <UIGroupMemberSelector>Membership selector</UIGroupMemberSelector>
      <UIWCMUserContainer>User selector</UIWCMUserContainer>
    </title>
  </SelectUserOrGroup>
  <UIGroupMemberSelector>
    <label>
      <selectGroup>Select group</selectGroup>
      <selectMembership>Select membership</selectMembership>
    </label>
  </UIGroupMemberSelector>
  <UIPermissionInfo>
    <action>
      <title>
        <Edit>Edit</Edit>
        <Delete>Delete</Delete>
      </title>
    </action>
    <header>
      <usersOrGroups>User Or Group</usersOrGroups>
      <accessible>Access</accessible>
      <editable>Edit</editable>
      <action>Action</action>
    </header>
    <msg>
      <confirm-delete-permission>Are you sure to delete.</confirm-delete-permission>
      <no-permission-remove>Don't have permission.</no-permission-remove>
      <access-denied>Access denied.</access-denied>
    </msg>
  </UIPermissionInfo>
  <location>
    <title>
      <Browse />
    </title>
  </location>
  <publication>
    <title>
      <BrowsePublication />
    </title>
  </publication>
  <!--/n  ##################################################################################
  #  org.exoplatform.wcm.presentation.acp.config.selector.UIWebContentPathSelector #
  ##################################################################################
  -->
  <UIWebContentSelectorForm>
    <title>Select a webcontent and publish to this page</title>
    <label>
      <path />
      <publication>publication</publication>
      <PublicationPath />
    </label>
    <action>
      <Save>Save</Save>
      <Back>Back</Back>
    </action>
  </UIWebContentSelectorForm>
  <!--/n  ##################################################################################
  -->
  <UIWizardWelcome>
    <WelcomeLabel>Welcome you to eXo Web Content Creation Wizard !</WelcomeLabel>
    <WelcomeText>This Wizard will walk you through the creation of a new web content. It guides you through entering the information necessary for creating a new web content. When you are done entering the required information, you will be prompted to save your new web content. And, it is the easy way by click on the "Back" and "Next" buttons or select the step number to navigate the steps.</WelcomeText>
  </UIWizardWelcome>
  <!--/n  ######################################################################
  # 		org.exoplatform.wcm.presentation.acp.config.UITagging		 #
  ######################################################################
  -->
  <UITagging>
    <label>
      <TagNames>Tag names</TagNames>
      <LinkedTags>Linked tags</LinkedTags>
    </label>
    <action>
      <AddTags>Add Tags</AddTags>
    </action>
    <msg>
      <tag-name-empty>Your tag name is empty</tag-name-empty>
      <name-exist>Tag name is existing</name-exist>
      <tagName-invalid>Have some invalid characters in tag name.</tagName-invalid>
      <tagName-too-long>One of the tags entered was too long. Tags must be less than 20 characters long</tagName-too-long>
    </msg>
    <introduction>
      <tagName>(Use "," separator to add multiple tag)</tagName>
    </introduction>
  </UITagging>
  <!--/n  ##############################################################################
  # 		org.exoplatform.wcm.presentation.acp.config.UIPermissionSetting		 #
  ##############################################################################
  -->
  <UIPermissionSetting>
    <label>
      <usersInputSet>UIPermissionSetting</usersInputSet>
      <usersStringInput>User Or Group</usersStringInput>
      <editable>Edit Right</editable>
      <accessible>Access Right</accessible>
    </label>
    <action>
      <Save>Save</Save>
      <Reset>Reset</Reset>
    </action>
    <msg>
      <userOrGroup-required>Require select user or group.</userOrGroup-required>
      <checkbox-require>You should set right.</checkbox-require>
    </msg>
  </UIPermissionSetting>
  <usersInputSet>
    <title>
      <SelectUser>Select user</SelectUser>
      <SelectMember>Select member</SelectMember>
      <AddAny>Add any permission</AddAny>
    </title>
  </usersInputSet>
  <!--/n  ##############################################################################
  # 		org.exoplatform.wcm.presentation.acp.config.UIPermissionInfo		 #
  ##############################################################################
  -->
  <!--/n  #############################################################
  -->
  <UIDocumentForm>
    <msg>
      <not-support>Not-support</not-support>
    </msg>
  </UIDocumentForm>
  <!--/n  #############################################################
  -->
  <UIPresentationContainer>
    <title>Quick Edit</title>
    <quickPrint>
    	<title>Print</title>
    	<print>Print</print>
    	<close>Close</close>
    </quickPrint>
  </UIPresentationContainer>
  <!--/n  #############################################################
  # org.exoplatform.wcm.webui.scv.config.UIDMSSelectorForm    #
  #############################################################
  -->
  <UIDMSSelectorForm>
    <label>
      <path />
      <publication>publication</publication>
      <PublicationPath />
    </label>
    <action>
      <Save>Save</Save>
      <Back>Back</Back>
    </action>
    <msg>
      <require-choose>Please choose an existing DMS document!</require-choose>
    </msg>
    <title>Select a document and publish to this page</title>
  </UIDMSSelectorForm>
  <!--/n  ##############################################################
  -->
  <UINameWebContentForm>
    <label>
      <name>name</name>
      <selectTemplate>Web content layout</selectTemplate>
    </label>
    <action>
      <Save>Save</Save>
      <Abort>Abort</Abort>
    </action>
    <msg>
      <non-lifecyclename>Please select a lifecycle name!</non-lifecyclename>
      <non-firstwhiteletter>can not use space letter in the first letter of name field!</non-firstwhiteletter>
    </msg>
  </UINameWebContentForm>
  <!--/n  ##########################################################################
  -->
  <UINonEditable>
    <action>
      <BackToView>Back to view mode</BackToView>
    </action>
  </UINonEditable>
  <UIQuickEditContainer>
    <action>
      <BackToNormalWizard>Back</BackToNormalWizard>
    </action>
    <back>Click on this to access advanced options for configuring the content</back>
  </UIQuickEditContainer>
  <UIPopupMenu>
    <msg>
      <node-locked>The item with path {0} or upper level is locked.</node-locked>
    </msg>
  </UIPopupMenu>
  <!--/n  ###########################################################################
  -->
  <UIWCMPublicationGrid>
    <header>
      <LifecycleName>Lifecycle name</LifecycleName>
      <LifecycleDesc>Lifecycle description</LifecycleDesc>
      <action>Select</action>
    </header>
    <non-lifecyclename>No lifecycle is selected</non-lifecyclename>
    <lifecyclename-selected>This webcontent will be published by this publication lifecycle</lifecyclename-selected>
  </UIWCMPublicationGrid>
  <UIQuickEditWebContentForm>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
  </UIQuickEditWebContentForm>
  <UIStartEditionInPageWizard>
    <link>Click here to change into edit content interface!</link>
  </UIStartEditionInPageWizard>
</bundle>

