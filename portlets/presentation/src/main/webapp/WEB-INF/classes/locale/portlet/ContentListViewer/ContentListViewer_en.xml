<?xml version="1.0" encoding="UTF-8"?>
<bundle>
	<UIFormInputSetWithAction>
		<msg>
			<confirm-delete>Are you sure to delete this category from list?</confirm-delete>
		</msg>
	</UIFormInputSetWithAction>
	<FolderPathInputSet>
		<title>
			<SelectFolderPath>Select folder path</SelectFolderPath>
		</title>
	</FolderPathInputSet>
  <UICLVConfig>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
      <Down>Down</Down>
      <Up>Up</Up>      
      <Delete>Delete</Delete>            
    </action>
    <label>
    	<ContentSelectionBlock>Contents selection</ContentSelectionBlock>
    	<DisplaySettingBlock>Display setting</DisplaySettingBlock>    	
      <FolderPathInputSet>UIViewerManagementForm</FolderPathInputSet>
      <FolderPathInput>Folder path</FolderPathInput>
      <FormViewTemplate>Viewer template</FormViewTemplate>
      <PaginatorTemplate>Paginator template</PaginatorTemplate>
      <ItemsPerPage>Items per page</ItemsPerPage>
      <ViewerButtonQuickEdit>Show quick edit button</ViewerButtonQuickEdit>
      <ViewerButtonRefresh>Show refresh button</ViewerButtonRefresh>
      <ViewerReadmore>Read more</ViewerReadmore>
      <ViewerThumbnailsView>Show image</ViewerThumbnailsView>
      <ViewerTitle>Show title</ViewerTitle>
      <ViewerDateCreated>Show date created</ViewerDateCreated>
      <ViewerSummary>Show summary</ViewerSummary>
      <Header>Header</Header>
      <ViewerLink>Show link</ViewerLink>
      <ViewerHeader>Show header</ViewerHeader>
      <ViewerMode>Mode</ViewerMode>
      <ManualViewerMode>Manual</ManualViewerMode>
      <AutoViewerMode>Automatic</AutoViewerMode>
      <OrderBy>Order by</OrderBy>
      <OrderByTitle>Title</OrderByTitle>
      <OrderByDateCreated>Date created</OrderByDateCreated>
      <OrderByDateModified>Date modified</OrderByDateModified>
      <OrderByDatePublished>Date published</OrderByDatePublished>      
      <OrderDesc>Descendant</OrderDesc>
      <OrderAsc>Ascendant</OrderAsc>   
      <ContentPath>Content</ContentPath>  
      <UICLVBasePathInputSetAction>Base path</UICLVBasePathInputSetAction> 
    </label>
    
    <title>Select folder to display its children documents or webcontent</title>
  </UICLVConfig>
  <UIMessageBoard>
    <msg>
      <folder-not-found>This viewer has not been configured yet.</folder-not-found>
      <no-permission>Folder access denied.</no-permission>
      <folder-empty>There is no content to show.</folder-empty>
      <error-nodetype>Unknown error.</error-nodetype>
      <saving-success>Successful. You can see the effect when changing to view mode.</saving-success>
      <empty-folder-path>You must select a folder.</empty-folder-path>
      <not-valid-action>Viewer mode is not suitable with contents selected. You should change viewer mode.</not-valid-action>
      <none-action-excuted>No action will be executed.</none-action-excuted>
      <add-node-error>Error when add node.</add-node-error>
      <node-publication-error>Error when perform get node publication life cycle.</node-publication-error>
      <show-draft-button>Error when show draft button.</show-draft-button>
      <get-content-icon>Error when get content icon.</get-content-icon>
      <get-illustration-image>Error when get illustration image.</get-illustration-image>
    </msg>
  </UIMessageBoard>
  <UIFolderTreeList>
    <title>Select a folder to display its children as content</title>
  </UIFolderTreeList>
  <UIFolderExplorer>
    <title>Below is the possible folder can be browsed</title>
  </UIFolderExplorer>
  <UIStartEditionInPageWizard>
    <msg>
      <edit-portlet>Click here to edit this portlet</edit-portlet>
    </msg>
  </UIStartEditionInPageWizard>
  <UIViewerManagementPopupWindow>
    <title>
      <UICLVConfig>Content list viewer management</UICLVConfig>
      <UIContentEdittingPopup>Content editting</UIContentEdittingPopup>
    </title>
  </UIViewerManagementPopupWindow>
  <UICLVContentSelectedGrid>
		<action>
			<SaveCategories>Save</SaveCategories>
			<Cancel>Cancel</Cancel>
		</action>
  </UICLVContentSelectedGrid>
  <UISelectedContentGrid>
		<action>
			<SaveCategories>Save</SaveCategories>
			<Cancel>Cancel</Cancel>
			<Down>Down</Down>
			<Up>Up</Up>
			<Delete>Delete</Delete>			
		</action>
		<label>
			<header>List selected content</header>
			<name>Name</name>
			<delete>Delete</delete>
		</label>
		<msg>
			<confirm-delete>Are you sure to delete this content from selected list?</confirm-delete>
		</msg>
  </UISelectedContentGrid>
  <FolderPathSelectorPopupWindow>
  	<title>
  		<UICLVFolderSelector>Select a folder to view it's content</UICLVFolderSelector>
