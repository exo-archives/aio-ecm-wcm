  </FolderPathSelectorPopupWindow>
  <CorrectContentSelectorPopupWindow>
  	<title>
  		<UICLVContentSelector>Contents manually selection</UICLVContentSelector>
  		<UIContentSelectorMulti>Content multi selector</UIContentSelectorMulti>
  	</title>
  </CorrectContentSelectorPopupWindow>
  <UICLVFolderSelector>
  	<action>
  		<Close>Close</Close>
  	</action>
  </UICLVFolderSelector>
  <UIContentSelectionPanel>
  	<label>
  		<header>Contents bellow can be selected</header>
  		<name>Name</name>
